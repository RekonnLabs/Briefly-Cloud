{
  "name": "briefly-cloud",
  "version": "1.0.0",
  "description": "AI-powered productivity assistant with unified Next.js architecture",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "node scripts/ci-no-test-imports.mjs",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "node scripts/clean-build.js",
    "clean:build": "node scripts/clean-build.js && npm run build",
    "test": "jest --runInBand",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --runInBand",
    "test:integration": "jest tests/integration --runInBand",
    "test:performance": "jest tests/performance --runInBand",
    "test:middleware": "jest tests/integration/api-middleware.test.ts --runInBand",
    "test:oauth": "jest tests/integration/oauth-token-management.test.ts --runInBand",
    "test:oauth-refinements": "node scripts/run-oauth-tests.mjs",
    "test:oauth-response": "jest tests/integration/oauth-response-consistency.test.ts --runInBand",
    "test:oauth-state": "jest tests/integration/oauth-state-verification.test.ts --runInBand",
    "test:oauth-cache": "jest tests/integration/oauth-cache-prevention.test.ts --runInBand",
    "test:oauth-e2e": "jest tests/integration/oauth-end-to-end.test.ts --runInBand",
    "test:google-scopes": "jest tests/integration/google-drive-scope-verification.test.ts --runInBand",
    "test:storage": "jest tests/integration/cloud-storage-integration.test.ts --runInBand",
    "test:backend": "jest tests/integration tests/performance --runInBand",
    "test:google-picker": "jest --testPathPatterns=\"google-picker|GooglePicker|register-files\" --runInBand",
    "test:google-picker:coverage": "jest --testPathPatterns=\"google-picker|GooglePicker|register-files\" --coverage --runInBand",
    "test:google-picker:watch": "jest --testPathPatterns=\"google-picker|GooglePicker|register-files\" --watch",
    "security:audit": "npm audit --audit-level=moderate",
    "validate:environment": "node scripts/validate-environment.js",
    "launch:checklist": "node scripts/launch-checklist.js",
    "smoke:auth": "node scripts/smoke-auth.mjs"
  },
  "keywords": [
    "ai",
    "productivity",
    "documents",
    "chat",
    "nextjs",
    "typescript",
    "openai",
    "vector-search"
  ],
  "author": "RekonnLabs",
  "license": "MIT",
  "dependencies": {
    "@azure/msal-node": "^3.7.0",
    "@radix-ui/react-accordion": "^1.2.11",
    "@radix-ui/react-alert-dialog": "^1.1.14",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.2",
    "@radix-ui/react-dialog": "^1.1.14",
    "@radix-ui/react-dropdown-menu": "^2.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.14",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-select": "^2.2.5",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.5",
    "@radix-ui/react-switch": "^1.2.5",
    "@radix-ui/react-tabs": "^1.1.12",
    "@radix-ui/react-toast": "^1.2.14",
    "@radix-ui/react-tooltip": "^1.2.7",
    "@sentry/nextjs": "^8.15.0",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.56.0",
    "@upstash/ratelimit": "^2.0.6",
    "@upstash/redis": "^1.35.3",
    "@vercel/analytics": "^1.2.2",
    "axios": "^1.11.0",
    "chromadb": "^3.0.10",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.1",
    "formidable": "^3.5.4",
    "googleapis": "^155.0.0",
    "lru-cache": "^10.2.0",
    "lucide-react": "^0.537.0",
    "mammoth": "^1.10.0",
    "next": "15.4.6",
    "openai": "^5.12.0",
    "pdf-parse": "^1.1.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sonner": "^2.0.7",
    "stripe": "^18.4.0",
    "tailwind-merge": "^3.3.1",
    "xlsx": "^0.18.5",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/formidable": "^3.4.5",
    "@types/jest": "^30.0.0",
    "@types/node": "^20",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
