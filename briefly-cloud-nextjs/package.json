    "@types/jest": "^30.0.0",
    "@types/node": "^20.19.13",
    "@types/pdf-parse": "^1.1.5",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "eslint": "^9",
    "eslint-config-next": "15.4.6",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "tailwindcss": "^4",
    "typescript": "^5.9.2"
