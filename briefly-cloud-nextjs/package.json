    "deploy:apideck-rls:verify": "node scripts/deploy-apideck-rls-fix.js --verify-only",
    "deploy:apideck-rls:test": "node scripts/deploy-apideck-rls-fix.js --test-only",
    "deploy:database": "node scripts/deploy-database-migration.js",
    "deploy:database:validate": "node scripts/deploy-database-migration.js --validate-only",
    "deploy:database:test": "node scripts/deploy-database-migration.js --test-only",
