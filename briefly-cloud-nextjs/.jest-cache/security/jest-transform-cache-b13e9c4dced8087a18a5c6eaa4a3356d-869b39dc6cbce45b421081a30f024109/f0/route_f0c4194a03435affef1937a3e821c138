        /* istanbul ignore next */
        (cov_2i1subnayr().b[24][1]++, 0)) / (
        /* istanbul ignore next */
        (cov_2i1subnayr().b[25][0]++, userProfile.storage_limit_bytes) ||
        /* istanbul ignore next */
        (cov_2i1subnayr().b[25][1]++, tierLimits.storage_bytes)) * 100),
        remaining: Math.max(0, (
        /* istanbul ignore next */
        (cov_2i1subnayr().b[26][0]++, userProfile.storage_limit_bytes) ||
        /* istanbul ignore next */
        (cov_2i1subnayr().b[26][1]++, tierLimits.storage_bytes)) - (
        /* istanbul ignore next */
        (cov_2i1subnayr().b[27][0]++, userProfile.storage_used_bytes) ||
        /* istanbul ignore next */
        (cov_2i1subnayr().b[27][1]++, 0))),
        used_formatted: formatBytes(
        /* istanbul ignore next */
        (cov_2i1subnayr().b[28][0]++, userProfile.storage_used_bytes) ||
        /* istanbul ignore next */
        (cov_2i1subnayr().b[28][1]++, 0)),
        limit_formatted: formatBytes(
        /* istanbul ignore next */
        (cov_2i1subnayr().b[29][0]++, userProfile.storage_limit_bytes) ||
        /* istanbul ignore next */
        (cov_2i1subnayr().b[29][1]++, tierLimits.storage_bytes))
      }
    });
    let usageHistory =
    /* istanbul ignore next */
    (cov_2i1subnayr().s[23]++, null);
    // Get usage history if requested
    /* istanbul ignore next */
    cov_2i1subnayr().s[24]++;
    if (includeHistory) {
      /* istanbul ignore next */
      cov_2i1subnayr().b[30][0]++;
      const pagination =
      /* istanbul ignore next */
      (cov_2i1subnayr().s[25]++, (0, _apiutils.parsePaginationParams)(url.searchParams));
      // Calculate date range based on timeframe
      const now =
      /* istanbul ignore next */
      (cov_2i1subnayr().s[26]++, new Date());
      let startDate;
      /* istanbul ignore next */
      cov_2i1subnayr().s[27]++;
      switch (timeframe) {
        case 'last_7_days':
          /* istanbul ignore next */
          cov_2i1subnayr().b[31][0]++;
          cov_2i1subnayr().s[28]++;
          startDate = new Date(now.getTime() - 7 * 24 * 60 * 60 * 1000);
          /* istanbul ignore next */
          cov_2i1subnayr().s[29]++;
          break;
        case 'last_30_days':
          /* istanbul ignore next */
          cov_2i1subnayr().b[31][1]++;
          cov_2i1subnayr().s[30]++;
          startDate = new Date(now.getTime() - 30 * 24 * 60 * 60 * 1000);
          /* istanbul ignore next */
          cov_2i1subnayr().s[31]++;
          break;
        case 'current_month':
          /* istanbul ignore next */
          cov_2i1subnayr().b[31][2]++;
          cov_2i1subnayr().s[32]++;
          startDate = new Date(now.getFullYear(), now.getMonth(), 1);
          /* istanbul ignore next */
          cov_2i1subnayr().s[33]++;
          break;
        case 'last_month':
          /* istanbul ignore next */
          cov_2i1subnayr().b[31][3]++;
          cov_2i1subnayr().s[34]++;
          startDate = new Date(now.getFullYear(), now.getMonth() - 1, 1);
          /* istanbul ignore next */
          cov_2i1subnayr().s[35]++;
          break;
        default:
          /* istanbul ignore next */
          cov_2i1subnayr().b[31][4]++;
          cov_2i1subnayr().s[36]++;
          startDate = new Date(now.getTime() - 30 * 24 * 60 * 60 * 1000);
      }
      // Get usage logs
      const {
        data: usageLogs,
        error: logsError,
        count
      } =
      /* istanbul ignore next */
      (cov_2i1subnayr().s[37]++, await supabase.from('usage_logs').select('*', {
        count: 'exact'
      }).eq('user_id', user.id).gte('created_at', startDate.toISOString()).order('created_at', {
        ascending: false
      }).range(pagination.offset, pagination.offset + pagination.limit - 1));
      /* istanbul ignore next */
      cov_2i1subnayr().s[38]++;
      if (logsError) {
        /* istanbul ignore next */
        cov_2i1subnayr().b[32][0]++;
        cov_2i1subnayr().s[39]++;
        console.error('Usage logs fetch error:', logsError);
      } else {
        /* istanbul ignore next */
        cov_2i1subnayr().b[32][1]++;
        cov_2i1subnayr().s[40]++;
        usageHistory = (0, _apiutils.createPaginatedResponse)(
        /* istanbul ignore next */
        (cov_2i1subnayr().b[33][0]++, usageLogs) ||
        /* istanbul ignore next */
        (cov_2i1subnayr().b[33][1]++, []),
        /* istanbul ignore next */
        (cov_2i1subnayr().b[34][0]++, count) ||
        /* istanbul ignore next */
        (cov_2i1subnayr().b[34][1]++, 0), pagination);
      }
    } else
    /* istanbul ignore next */
    {
      cov_2i1subnayr().b[30][1]++;
    }
    // Get usage warnings
    const warnings =
    /* istanbul ignore next */
    (cov_2i1subnayr().s[41]++, []);
    /* istanbul ignore next */
    cov_2i1subnayr().s[42]++;
    Object.entries(currentUsage).forEach(([key, usage]) => {
      /* istanbul ignore next */
      cov_2i1subnayr().f[2]++;
      cov_2i1subnayr().s[43]++;
      if (usage.percentage >= 90) {
        /* istanbul ignore next */
        cov_2i1subnayr().b[35][0]++;
        cov_2i1subnayr().s[44]++;
        warnings.push({
          type: key,
          level: 'critical',
          message: `You have used ${usage.percentage}% of your ${key.replace('_', ' ')} limit`
        });
      } else {
        /* istanbul ignore next */
        cov_2i1subnayr().b[35][1]++;
        cov_2i1subnayr().s[45]++;
        if (usage.percentage >= 80) {
          /* istanbul ignore next */
          cov_2i1subnayr().b[36][0]++;
          cov_2i1subnayr().s[46]++;
          warnings.push({
            type: key,
            level: 'warning',
            message: `You have used ${usage.percentage}% of your ${key.replace('_', ' ')} limit`
          });
        } else
        /* istanbul ignore next */
        {
          cov_2i1subnayr().b[36][1]++;
        }
      }
    });
    // Log usage
    /* istanbul ignore next */
    cov_2i1subnayr().s[47]++;
    (0, _logger.logApiUsage)(user.id, '/api/user/usage', 'usage_view', {
      timeframe,
      include_history: includeHistory
    });
    const responseData =
    /* istanbul ignore next */
    (cov_2i1subnayr().s[48]++, {
      subscription_tier: userProfile.subscription_tier,
      usage_reset_date: userProfile.usage_reset_date,
      current_usage: currentUsage,
      warnings,
      tier_limits: tierLimits,
      ...(
      /* istanbul ignore next */
      (cov_2i1subnayr().b[37][0]++, usageHistory) &&
      /* istanbul ignore next */
      (cov_2i1subnayr().b[37][1]++, {
        usage_history: usageHistory
      }))
    });
    /* istanbul ignore next */
    cov_2i1subnayr().s[49]++;
    return _apiutils.ApiResponse.success(responseData);
  } catch (error) {
    /* istanbul ignore next */
    cov_2i1subnayr().s[50]++;
    console.error('Usage handler error:', error);
    /* istanbul ignore next */
    cov_2i1subnayr().s[51]++;
    return _apiutils.ApiResponse.internalError('Failed to process usage request');
  }
}
// Helper function to format bytes
function formatBytes(bytes) {
  /* istanbul ignore next */
  cov_2i1subnayr().f[3]++;
  const units =
  /* istanbul ignore next */
  (cov_2i1subnayr().s[52]++, ['B', 'KB', 'MB', 'GB', 'TB']);
  let size =
  /* istanbul ignore next */
  (cov_2i1subnayr().s[53]++, bytes);
  let unitIndex =
  /* istanbul ignore next */
  (cov_2i1subnayr().s[54]++, 0);
  /* istanbul ignore next */
  cov_2i1subnayr().s[55]++;
  while (
  /* istanbul ignore next */
  (cov_2i1subnayr().b[38][0]++, size >= 1024) &&
  /* istanbul ignore next */
  (cov_2i1subnayr().b[38][1]++, unitIndex < units.length - 1)) {
    /* istanbul ignore next */
    cov_2i1subnayr().s[56]++;
    size /= 1024;
    /* istanbul ignore next */
    cov_2i1subnayr().s[57]++;
    unitIndex++;
  }
  /* istanbul ignore next */
  cov_2i1subnayr().s[58]++;
  return `${size.toFixed(1)} ${units[unitIndex]}`;
}
const GET =
/* istanbul ignore next */
(cov_2i1subnayr().s[59]++, (0, _apimiddleware.createProtectedApiHandler)(getUserUsageHandler, {
  rateLimit: _ratelimit.rateLimitConfigs.general,
  logging: {
    enabled: true,
    includeBody: false
  }
}));
