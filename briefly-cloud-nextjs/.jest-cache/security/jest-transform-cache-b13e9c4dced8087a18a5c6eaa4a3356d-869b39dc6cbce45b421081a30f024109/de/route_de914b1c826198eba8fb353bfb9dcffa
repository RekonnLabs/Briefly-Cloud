  (cov_p0m9pxpko().s[25]++, new Date().toISOString());
  const alerts =
  /* istanbul ignore next */
  (cov_p0m9pxpko().s[26]++, []);
  // Check all services in parallel
  const [databaseHealth, openaiHealth, supabaseHealth, chromadbHealth] =
  /* istanbul ignore next */
  (cov_p0m9pxpko().s[27]++, await Promise.allSettled([checkDatabaseHealth(), checkOpenAIHealth(), checkSupabaseHealth(), checkChromaDBHealth()]));
  const services =
  /* istanbul ignore next */
  (cov_p0m9pxpko().s[28]++, {
    database: databaseHealth.status === 'fulfilled' ?
    /* istanbul ignore next */
    (cov_p0m9pxpko().b[6][0]++, databaseHealth.value) :
    /* istanbul ignore next */
    (cov_p0m9pxpko().b[6][1]++, {
      status: 'unhealthy',
      responseTime: 0,
      error: databaseHealth.status === 'rejected' ?
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[7][0]++, databaseHealth.reason?.message) :
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[7][1]++, 'Unknown error')
    }),
    openai: openaiHealth.status === 'fulfilled' ?
    /* istanbul ignore next */
    (cov_p0m9pxpko().b[8][0]++, openaiHealth.value) :
    /* istanbul ignore next */
    (cov_p0m9pxpko().b[8][1]++, {
      status: 'unhealthy',
      responseTime: 0,
      error: openaiHealth.status === 'rejected' ?
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[9][0]++, openaiHealth.reason?.message) :
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[9][1]++, 'Unknown error')
    }),
    supabase: supabaseHealth.status === 'fulfilled' ?
    /* istanbul ignore next */
    (cov_p0m9pxpko().b[10][0]++, supabaseHealth.value) :
    /* istanbul ignore next */
    (cov_p0m9pxpko().b[10][1]++, {
      status: 'unhealthy',
      responseTime: 0,
      error: supabaseHealth.status === 'rejected' ?
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[11][0]++, supabaseHealth.reason?.message) :
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[11][1]++, 'Unknown error')
    })
  });
  // Add ChromaDB if configured
  /* istanbul ignore next */
  cov_p0m9pxpko().s[29]++;
  if (
  /* istanbul ignore next */
  (cov_p0m9pxpko().b[13][0]++, chromadbHealth.status === 'fulfilled') &&
  /* istanbul ignore next */
  (cov_p0m9pxpko().b[13][1]++, chromadbHealth.value)) {
    /* istanbul ignore next */
    cov_p0m9pxpko().b[12][0]++;
    cov_p0m9pxpko().s[30]++;
    services.chromadb = chromadbHealth.value;
  } else {
    /* istanbul ignore next */
    cov_p0m9pxpko().b[12][1]++;
    cov_p0m9pxpko().s[31]++;
    if (chromadbHealth.status === 'rejected') {
      /* istanbul ignore next */
      cov_p0m9pxpko().b[14][0]++;
      cov_p0m9pxpko().s[32]++;
      services.chromadb = {
        status: 'unhealthy',
        responseTime: 0,
        error:
        /* istanbul ignore next */
        (cov_p0m9pxpko().b[15][0]++, chromadbHealth.reason?.message) ||
        /* istanbul ignore next */
        (cov_p0m9pxpko().b[15][1]++, 'Unknown error')
      };
    } else
    /* istanbul ignore next */
    {
      cov_p0m9pxpko().b[14][1]++;
    }
  }
  // Generate alerts based on service health
  /* istanbul ignore next */
  cov_p0m9pxpko().s[33]++;
  Object.entries(services).forEach(([serviceName, health]) => {
    /* istanbul ignore next */
    cov_p0m9pxpko().f[5]++;
    cov_p0m9pxpko().s[34]++;
    if (health.status === 'unhealthy') {
      /* istanbul ignore next */
      cov_p0m9pxpko().b[16][0]++;
      cov_p0m9pxpko().s[35]++;
      alerts.push({
        type: 'error',
        service: serviceName,
        message: `${serviceName} is unhealthy: ${
        /* istanbul ignore next */
        (cov_p0m9pxpko().b[17][0]++, health.error) ||
        /* istanbul ignore next */
        (cov_p0m9pxpko().b[17][1]++, 'Unknown error')}`,
        timestamp
      });
    } else {
      /* istanbul ignore next */
      cov_p0m9pxpko().b[16][1]++;
      cov_p0m9pxpko().s[36]++;
      if (health.status === 'degraded') {
        /* istanbul ignore next */
        cov_p0m9pxpko().b[18][0]++;
        cov_p0m9pxpko().s[37]++;
        alerts.push({
          type: 'warning',
          service: serviceName,
          message: `${serviceName} is degraded (response time: ${health.responseTime}ms)`,
          timestamp
        });
      } else
      /* istanbul ignore next */
      {
        cov_p0m9pxpko().b[18][1]++;
      }
    }
  });
  // Determine overall health
  const serviceStatuses =
  /* istanbul ignore next */
  (cov_p0m9pxpko().s[38]++, Object.values(services).map(s => {
    /* istanbul ignore next */
    cov_p0m9pxpko().f[6]++;
    cov_p0m9pxpko().s[39]++;
    return s.status;
  }));
  const hasUnhealthy =
  /* istanbul ignore next */
  (cov_p0m9pxpko().s[40]++, serviceStatuses.includes('unhealthy'));
  const hasDegraded =
  /* istanbul ignore next */
  (cov_p0m9pxpko().s[41]++, serviceStatuses.includes('degraded'));
  let overall;
  /* istanbul ignore next */
  cov_p0m9pxpko().s[42]++;
  if (hasUnhealthy) {
    /* istanbul ignore next */
    cov_p0m9pxpko().b[19][0]++;
    cov_p0m9pxpko().s[43]++;
    overall = 'unhealthy';
  } else {
    /* istanbul ignore next */
    cov_p0m9pxpko().b[19][1]++;
    cov_p0m9pxpko().s[44]++;
    if (hasDegraded) {
      /* istanbul ignore next */
      cov_p0m9pxpko().b[20][0]++;
      cov_p0m9pxpko().s[45]++;
      overall = 'degraded';
    } else {
      /* istanbul ignore next */
      cov_p0m9pxpko().b[20][1]++;
      cov_p0m9pxpko().s[46]++;
      overall = 'healthy';
    }
  }
  /* istanbul ignore next */
  cov_p0m9pxpko().s[47]++;
  return {
    timestamp,
    overall,
    services,
    alerts
  };
}
/**
 * Check database health
 */
async function checkDatabaseHealth() {
  /* istanbul ignore next */
  cov_p0m9pxpko().f[7]++;
  const startTime =
  /* istanbul ignore next */
  (cov_p0m9pxpko().s[48]++, Date.now());
  /* istanbul ignore next */
  cov_p0m9pxpko().s[49]++;
  try {
    const supabase =
    /* istanbul ignore next */
    (cov_p0m9pxpko().s[50]++, _supabase.supabaseAdmin);
    const {
      data,
      error
    } =
    /* istanbul ignore next */
    (cov_p0m9pxpko().s[51]++, await supabase.from('users').select('count').limit(1));
    const responseTime =
    /* istanbul ignore next */
    (cov_p0m9pxpko().s[52]++, Date.now() - startTime);
    /* istanbul ignore next */
    cov_p0m9pxpko().s[53]++;
    if (error) {
      /* istanbul ignore next */
      cov_p0m9pxpko().b[21][0]++;
      cov_p0m9pxpko().s[54]++;
      return {
        status: 'unhealthy',
        responseTime,
        error: error.message
      };
    } else
    /* istanbul ignore next */
    {
      cov_p0m9pxpko().b[21][1]++;
    }
    cov_p0m9pxpko().s[55]++;
    return {
      status: responseTime > 1000 ?
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[22][0]++, 'degraded') :
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[22][1]++, 'healthy'),
      responseTime
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_p0m9pxpko().s[56]++;
    return {
      status: 'unhealthy',
      responseTime: Date.now() - startTime,
      error: error instanceof Error ?
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[23][0]++, error.message) :
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[23][1]++, 'Database connection failed')
    };
  }
}
/**
 * Check OpenAI API health
 */
async function checkOpenAIHealth() {
  /* istanbul ignore next */
  cov_p0m9pxpko().f[8]++;
  const startTime =
  /* istanbul ignore next */
  (cov_p0m9pxpko().s[57]++, Date.now());
  /* istanbul ignore next */
  cov_p0m9pxpko().s[58]++;
  try {
    const response =
    /* istanbul ignore next */
    (cov_p0m9pxpko().s[59]++, await fetch('https://api.openai.com/v1/models', {
      method: 'GET',
      headers: {
        'Authorization': `Bearer ${process.env.OPENAI_API_KEY}`,
        'Content-Type': 'application/json'
      },
      signal: AbortSignal.timeout(10000) // 10 second timeout
    }));
    const responseTime =
    /* istanbul ignore next */
    (cov_p0m9pxpko().s[60]++, Date.now() - startTime);
    /* istanbul ignore next */
    cov_p0m9pxpko().s[61]++;
    if (!response.ok) {
      /* istanbul ignore next */
      cov_p0m9pxpko().b[24][0]++;
      cov_p0m9pxpko().s[62]++;
      return {
        status: 'unhealthy',
        responseTime,
        error: `HTTP ${response.status}: ${response.statusText}`
      };
    } else
    /* istanbul ignore next */
    {
      cov_p0m9pxpko().b[24][1]++;
    }
    cov_p0m9pxpko().s[63]++;
    return {
      status: responseTime > 3000 ?
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[25][0]++, 'degraded') :
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[25][1]++, 'healthy'),
      responseTime
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_p0m9pxpko().s[64]++;
    return {
      status: 'unhealthy',
      responseTime: Date.now() - startTime,
      error: error instanceof Error ?
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[26][0]++, error.message) :
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[26][1]++, 'OpenAI API connection failed')
    };
  }
}
