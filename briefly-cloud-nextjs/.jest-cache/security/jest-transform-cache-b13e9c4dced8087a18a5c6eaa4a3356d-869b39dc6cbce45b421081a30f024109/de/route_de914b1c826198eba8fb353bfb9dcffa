  cov_p0m9pxpko().s[74]++;
  if (!process.env.CHROMADB_URL) {
    /* istanbul ignore next */
    cov_p0m9pxpko().b[30][0]++;
    cov_p0m9pxpko().s[75]++;
    return null;
  } else
  /* istanbul ignore next */
  {
    cov_p0m9pxpko().b[30][1]++;
  }
  const startTime =
  /* istanbul ignore next */
  (cov_p0m9pxpko().s[76]++, Date.now());
  /* istanbul ignore next */
  cov_p0m9pxpko().s[77]++;
  try {
    const response =
    /* istanbul ignore next */
    (cov_p0m9pxpko().s[78]++, await fetch(`${process.env.CHROMADB_URL}/api/v1/heartbeat`, {
      method: 'GET',
      headers: {
        'Content-Type': 'application/json',
        ...(
        /* istanbul ignore next */
        (cov_p0m9pxpko().b[31][0]++, process.env.CHROMADB_API_KEY) &&
        /* istanbul ignore next */
        (cov_p0m9pxpko().b[31][1]++, {
          'Authorization': `Bearer ${process.env.CHROMADB_API_KEY}`
        }))
      },
      signal: AbortSignal.timeout(10000)
    }));
    const responseTime =
    /* istanbul ignore next */
    (cov_p0m9pxpko().s[79]++, Date.now() - startTime);
    /* istanbul ignore next */
    cov_p0m9pxpko().s[80]++;
    if (!response.ok) {
      /* istanbul ignore next */
      cov_p0m9pxpko().b[32][0]++;
      cov_p0m9pxpko().s[81]++;
      return {
        status: 'unhealthy',
        responseTime,
        error: `HTTP ${response.status}: ${response.statusText}`
      };
    } else
    /* istanbul ignore next */
    {
      cov_p0m9pxpko().b[32][1]++;
    }
    cov_p0m9pxpko().s[82]++;
    return {
      status: responseTime > 3000 ?
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[33][0]++, 'degraded') :
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[33][1]++, 'healthy'),
      responseTime
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_p0m9pxpko().s[83]++;
    return {
      status: 'unhealthy',
      responseTime: Date.now() - startTime,
      error: error instanceof Error ?
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[34][0]++, error.message) :
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[34][1]++, 'ChromaDB connection failed')
    };
  }
}
/**
 * Store health check results in database
 */
async function storeHealthCheckResults(healthStatus) {
  /* istanbul ignore next */
  cov_p0m9pxpko().f[11]++;
  cov_p0m9pxpko().s[84]++;
  try {
    const supabase =
    /* istanbul ignore next */
    (cov_p0m9pxpko().s[85]++, _supabase.supabaseAdmin);
    /* istanbul ignore next */
    cov_p0m9pxpko().s[86]++;
    await supabase.from('health_checks').insert({
      timestamp: healthStatus.timestamp,
      overall_status: healthStatus.overall,
      services: healthStatus.services,
      alerts: healthStatus.alerts,
      created_at: new Date().toISOString()
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_p0m9pxpko().s[87]++;
    _logger.logger.error('Failed to store health check results', {
      error: error instanceof Error ?
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[35][0]++, error.message) :
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[35][1]++, 'Unknown error')
    });
  }
}
/**
 * Send health alerts via configured channels
 */
async function sendHealthAlerts(healthStatus) {
  /* istanbul ignore next */
  cov_p0m9pxpko().f[12]++;
  cov_p0m9pxpko().s[88]++;
  try {
    /* istanbul ignore next */
    cov_p0m9pxpko().s[89]++;
    // In a real implementation, you would send alerts via:
    // - Email (SendGrid, AWS SES)
    // - Slack (Webhook)
    // - Discord (Webhook)
    // - PagerDuty (API)
    // - SMS (Twilio)
    _logger.logger.warn('Health alerts triggered', {
      status: healthStatus.overall,
      alerts: healthStatus.alerts
    });
    // Example: Send to webhook (replace with actual implementation)
    /* istanbul ignore next */
    cov_p0m9pxpko().s[90]++;
    if (process.env.ALERT_WEBHOOK_URL) {
      /* istanbul ignore next */
      cov_p0m9pxpko().b[36][0]++;
      cov_p0m9pxpko().s[91]++;
      await fetch(process.env.ALERT_WEBHOOK_URL, {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json'
        },
        body: JSON.stringify({
          text: ` Briefly Cloud Health Alert: System is ${healthStatus.overall}`,
          attachments: [{
            color: healthStatus.overall === 'unhealthy' ?
            /* istanbul ignore next */
            (cov_p0m9pxpko().b[37][0]++, 'danger') :
            /* istanbul ignore next */
            (cov_p0m9pxpko().b[37][1]++, 'warning'),
            fields: healthStatus.alerts.map(alert => {
              /* istanbul ignore next */
              cov_p0m9pxpko().f[13]++;
              cov_p0m9pxpko().s[92]++;
              return {
                title: `${alert.service} - ${alert.type}`,
                value: alert.message,
                short: false
              };
            })
          }]
        })
      });
    } else
    /* istanbul ignore next */
    {
      cov_p0m9pxpko().b[36][1]++;
    }
  } catch (error) {
    /* istanbul ignore next */
    cov_p0m9pxpko().s[93]++;
    _logger.logger.error('Failed to send health alerts', {
      error: error instanceof Error ?
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[38][0]++, error.message) :
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[38][1]++, 'Unknown error')
    });
  }
}
/**
 * Send critical alert for system failures
 */
async function sendCriticalAlert(message, error) {
  /* istanbul ignore next */
  cov_p0m9pxpko().f[14]++;
  cov_p0m9pxpko().s[94]++;
  try {
    /* istanbul ignore next */
    cov_p0m9pxpko().s[95]++;
    _logger.logger.error('Critical system alert', {
      message,
      error: error instanceof Error ?
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[39][0]++, error.message) :
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[39][1]++, 'Unknown error')
    });
    // Send immediate critical alert
    /* istanbul ignore next */
    cov_p0m9pxpko().s[96]++;
    if (process.env.CRITICAL_ALERT_WEBHOOK_URL) {
      /* istanbul ignore next */
      cov_p0m9pxpko().b[40][0]++;
      cov_p0m9pxpko().s[97]++;
      await fetch(process.env.CRITICAL_ALERT_WEBHOOK_URL, {
        method: 'POST',
        headers: {
          'Content-Type': 'application/json'
        },
        body: JSON.stringify({
          text: ` CRITICAL: Briefly Cloud System Failure`,
          attachments: [{
            color: 'danger',
            fields: [{
              title: 'Error',
              value: message,
              short: false
            }, {
              title: 'Details',
              value: error instanceof Error ?
              /* istanbul ignore next */
              (cov_p0m9pxpko().b[41][0]++, error.message) :
              /* istanbul ignore next */
              (cov_p0m9pxpko().b[41][1]++, 'Unknown error'),
              short: false
            }]
          }]
        })
      });
    } else
    /* istanbul ignore next */
    {
      cov_p0m9pxpko().b[40][1]++;
    }
  } catch (alertError) {
    /* istanbul ignore next */
    cov_p0m9pxpko().s[98]++;
    _logger.logger.error('Failed to send critical alert', {
      originalError: error instanceof Error ?
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[42][0]++, error.message) :
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[42][1]++, 'Unknown error'),
      alertError: alertError instanceof Error ?
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[43][0]++, alertError.message) :
      /* istanbul ignore next */
      (cov_p0m9pxpko().b[43][1]++, 'Unknown error')
    });
  }
}
async function GET(request) {
  /* istanbul ignore next */
  cov_p0m9pxpko().f[15]++;
  cov_p0m9pxpko().s[99]++;
  try {
    const supabase =
    /* istanbul ignore next */
    (cov_p0m9pxpko().s[100]++, _supabase.supabaseAdmin);
    const {
      data: latestCheck
    } =
    /* istanbul ignore next */
    (cov_p0m9pxpko().s[101]++, await supabase.from('health_checks').select('*').order('created_at', {
      ascending: false
    }).limit(1).single());
    /* istanbul ignore next */
    cov_p0m9pxpko().s[102]++;
    if (!latestCheck) {
      /* istanbul ignore next */
      cov_p0m9pxpko().b[44][0]++;
      cov_p0m9pxpko().s[103]++;
      return (0, _apiutils.createApiResponse)({
        message: 'No health check data available',
        status: 'unknown'
      });
    } else
    /* istanbul ignore next */
    {
      cov_p0m9pxpko().b[44][1]++;
    }
    cov_p0m9pxpko().s[104]++;
    return (0, _apiutils.createApiResponse)({
      timestamp: latestCheck.timestamp,
      status: latestCheck.overall_status,
      services: latestCheck.services,
      alerts: latestCheck.alerts
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_p0m9pxpko().s[105]++;
    return (0, _apiutils.createErrorResponse)('Failed to get health check status', 500);
  }
}
