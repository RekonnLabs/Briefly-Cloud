  /**
  * Group usage data for analytics
  */
  groupUsageData(logs, groupBy) {
    /* istanbul ignore next */
    cov_1ymf0zp9u0().f[16]++;
    const grouped =
    /* istanbul ignore next */
    (cov_1ymf0zp9u0().s[96]++, new Map());
    /* istanbul ignore next */
    cov_1ymf0zp9u0().s[97]++;
    logs.forEach(log => {
      /* istanbul ignore next */
      cov_1ymf0zp9u0().f[17]++;
      let key;
      /* istanbul ignore next */
      cov_1ymf0zp9u0().s[98]++;
      switch (groupBy) {
        case 'day':
          /* istanbul ignore next */
          cov_1ymf0zp9u0().b[27][0]++;
          cov_1ymf0zp9u0().s[99]++;
          key = new Date(log.created_at).toISOString().split('T')[0];
          /* istanbul ignore next */
          cov_1ymf0zp9u0().s[100]++;
          break;
        case 'hour':
          /* istanbul ignore next */
          cov_1ymf0zp9u0().b[27][1]++;
          const date =
          /* istanbul ignore next */
          (cov_1ymf0zp9u0().s[101]++, new Date(log.created_at));
          /* istanbul ignore next */
          cov_1ymf0zp9u0().s[102]++;
          key = `${date.toISOString().split('T')[0]} ${date.getHours()}:00`;
          /* istanbul ignore next */
          cov_1ymf0zp9u0().s[103]++;
          break;
        case 'action':
          /* istanbul ignore next */
          cov_1ymf0zp9u0().b[27][2]++;
          cov_1ymf0zp9u0().s[104]++;
          key = log.action;
          /* istanbul ignore next */
          cov_1ymf0zp9u0().s[105]++;
          break;
        default:
          /* istanbul ignore next */
          cov_1ymf0zp9u0().b[27][3]++;
          cov_1ymf0zp9u0().s[106]++;
          key = log.action;
      }
      /* istanbul ignore next */
      cov_1ymf0zp9u0().s[107]++;
      if (!grouped.has(key)) {
        /* istanbul ignore next */
        cov_1ymf0zp9u0().b[28][0]++;
        cov_1ymf0zp9u0().s[108]++;
        grouped.set(key, {
          key,
          total_quantity: 0,
          unique_users: new Set(),
          actions: new Map()
        });
      } else
      /* istanbul ignore next */
      {
        cov_1ymf0zp9u0().b[28][1]++;
      }
      const group =
      /* istanbul ignore next */
      (cov_1ymf0zp9u0().s[109]++, grouped.get(key));
      /* istanbul ignore next */
      cov_1ymf0zp9u0().s[110]++;
      group.total_quantity +=
      /* istanbul ignore next */
      (cov_1ymf0zp9u0().b[29][0]++, log.quantity) ||
      /* istanbul ignore next */
      (cov_1ymf0zp9u0().b[29][1]++, 1);
      /* istanbul ignore next */
      cov_1ymf0zp9u0().s[111]++;
      group.unique_users.add(log.user_id);
      /* istanbul ignore next */
      cov_1ymf0zp9u0().s[112]++;
      if (!group.actions.has(log.action)) {
        /* istanbul ignore next */
        cov_1ymf0zp9u0().b[30][0]++;
        cov_1ymf0zp9u0().s[113]++;
        group.actions.set(log.action, 0);
      } else
      /* istanbul ignore next */
      {
        cov_1ymf0zp9u0().b[30][1]++;
      }
      cov_1ymf0zp9u0().s[114]++;
      group.actions.set(log.action, group.actions.get(log.action) + (
      /* istanbul ignore next */
      (cov_1ymf0zp9u0().b[31][0]++, log.quantity) ||
      /* istanbul ignore next */
      (cov_1ymf0zp9u0().b[31][1]++, 1)));
    });
    // Convert to array and format
    /* istanbul ignore next */
    cov_1ymf0zp9u0().s[115]++;
    return Array.from(grouped.values()).map(group => {
      /* istanbul ignore next */
      cov_1ymf0zp9u0().f[18]++;
      cov_1ymf0zp9u0().s[116]++;
      return {
        key: group.key,
        total_quantity: group.total_quantity,
        unique_users: group.unique_users.size,
        actions: Object.fromEntries(group.actions)
      };
    });
  }
  /**
  * Reset usage counters (for monthly billing cycles)
  */
  async resetUsageCounters(userId) {
    /* istanbul ignore next */
    cov_1ymf0zp9u0().f[19]++;
    cov_1ymf0zp9u0().s[117]++;
    try {
      /* istanbul ignore next */
      cov_1ymf0zp9u0().s[118]++;
      if (userId) {
        /* istanbul ignore next */
        cov_1ymf0zp9u0().b[32][0]++;
        cov_1ymf0zp9u0().s[119]++;
        // Reset for specific user
        await _supabase.supabaseAdmin.from('app.users').update({
          chat_messages_count: 0,
          documents_uploaded: 0,
          api_calls_count: 0,
          usage_reset_date: new Date(Date.now() + 30 * 24 * 60 * 60 * 1000).toISOString(),
          updated_at: new Date().toISOString()
        }).eq('id', userId);
        /* istanbul ignore next */
        cov_1ymf0zp9u0().s[120]++;
        _logger.logger.info('Usage counters reset for user', {
          userId
        });
      } else {
        /* istanbul ignore next */
        cov_1ymf0zp9u0().b[32][1]++;
        // Reset for all users (monthly job)
        const {
          error
        } =
        /* istanbul ignore next */
        (cov_1ymf0zp9u0().s[121]++, await _supabase.supabaseAdmin.rpc('reset_monthly_usage'));
        /* istanbul ignore next */
        cov_1ymf0zp9u0().s[122]++;
        if (error) {
          /* istanbul ignore next */
          cov_1ymf0zp9u0().b[33][0]++;
          cov_1ymf0zp9u0().s[123]++;
          throw error;
        } else
        /* istanbul ignore next */
        {
          cov_1ymf0zp9u0().b[33][1]++;
        }
        cov_1ymf0zp9u0().s[124]++;
        _logger.logger.info('Monthly usage reset completed for all users');
      }
    } catch (error) {
      /* istanbul ignore next */
      cov_1ymf0zp9u0().s[125]++;
      _logger.logger.error('Failed to reset usage counters', {
        userId
      }, error);
      /* istanbul ignore next */
      cov_1ymf0zp9u0().s[126]++;
      throw _apierrors.createError.databaseError('Failed to reset usage counters', error);
    }
  }
}
