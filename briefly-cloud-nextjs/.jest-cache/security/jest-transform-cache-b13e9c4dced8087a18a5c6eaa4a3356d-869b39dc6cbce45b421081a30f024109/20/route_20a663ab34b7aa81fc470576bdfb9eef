        cov_1s4eda6hhm().s[23]++;
        _logger.logger.error('Failed to store alerts', {
          error
        });
        /* istanbul ignore next */
        cov_1s4eda6hhm().s[24]++;
        return (0, _apierrors.formatErrorResponse)('Failed to store alerts', 500);
      } else
      /* istanbul ignore next */
      {
        cov_1s4eda6hhm().b[2][1]++;
      }
      // Send critical alerts immediately
      const criticalAlerts =
      /* istanbul ignore next */
      (cov_1s4eda6hhm().s[25]++, alerts.filter(alert => {
        /* istanbul ignore next */
        cov_1s4eda6hhm().f[7]++;
        cov_1s4eda6hhm().s[26]++;
        return alert.severity === 'critical';
      }));
      /* istanbul ignore next */
      cov_1s4eda6hhm().s[27]++;
      if (criticalAlerts.length > 0) {
        /* istanbul ignore next */
        cov_1s4eda6hhm().b[3][0]++;
        cov_1s4eda6hhm().s[28]++;
        await sendCriticalAlerts(criticalAlerts);
      } else
      /* istanbul ignore next */
      {
        cov_1s4eda6hhm().b[3][1]++;
      }
      cov_1s4eda6hhm().s[29]++;
      _logger.logger.warn('Alerts stored', {
        count: alerts.length,
        criticalCount: criticalAlerts.length
      });
      /* istanbul ignore next */
      cov_1s4eda6hhm().s[30]++;
      return _server.NextResponse.json({
        success: true,
        message: `${alerts.length} alerts stored successfully`
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_1s4eda6hhm().s[31]++;
      _logger.logger.error('Alert storage error', {
        error
      });
      /* istanbul ignore next */
      cov_1s4eda6hhm().s[32]++;
      return (0, _apierrors.formatErrorResponse)('Invalid request', 400);
    }
  });
}
async function GET(request) {
  /* istanbul ignore next */
  cov_1s4eda6hhm().f[8]++;
  cov_1s4eda6hhm().s[33]++;
  return (0, _ratelimit.withRateLimit)(request, async () => {
    /* istanbul ignore next */
    cov_1s4eda6hhm().f[9]++;
    cov_1s4eda6hhm().s[34]++;
    try {
      const {
        searchParams
      } =
      /* istanbul ignore next */
      (cov_1s4eda6hhm().s[35]++, new URL(request.url));
      const hours =
      /* istanbul ignore next */
      (cov_1s4eda6hhm().s[36]++, parseInt(
      /* istanbul ignore next */
      (cov_1s4eda6hhm().b[4][0]++, searchParams.get('hours')) ||
      /* istanbul ignore next */
      (cov_1s4eda6hhm().b[4][1]++, '24')));
      const severity =
      /* istanbul ignore next */
      (cov_1s4eda6hhm().s[37]++, searchParams.get('severity'));
      const status =
      /* istanbul ignore next */
      (cov_1s4eda6hhm().s[38]++,
      /* istanbul ignore next */
      (cov_1s4eda6hhm().b[5][0]++, searchParams.get('status')) ||
      /* istanbul ignore next */
      (cov_1s4eda6hhm().b[5][1]++, 'active'));
      // Get alerts
      const supabase =
      /* istanbul ignore next */
      (cov_1s4eda6hhm().s[39]++, _supabase.supabaseAdmin);
      const cutoffDate =
      /* istanbul ignore next */
      (cov_1s4eda6hhm().s[40]++, new Date());
      /* istanbul ignore next */
      cov_1s4eda6hhm().s[41]++;
      cutoffDate.setHours(cutoffDate.getHours() - hours);
      let query =
      /* istanbul ignore next */
      (cov_1s4eda6hhm().s[42]++, supabase.from('monitoring_alerts').select('*').gte('created_at', cutoffDate.toISOString()).order('created_at', {
        ascending: false
      }));
      /* istanbul ignore next */
      cov_1s4eda6hhm().s[43]++;
      if (severity) {
        /* istanbul ignore next */
        cov_1s4eda6hhm().b[6][0]++;
        cov_1s4eda6hhm().s[44]++;
        query = query.eq('severity', severity);
      } else
      /* istanbul ignore next */
      {
        cov_1s4eda6hhm().b[6][1]++;
      }
      cov_1s4eda6hhm().s[45]++;
      if (status) {
        /* istanbul ignore next */
        cov_1s4eda6hhm().b[7][0]++;
        cov_1s4eda6hhm().s[46]++;
        query = query.eq('status', status);
      } else
      /* istanbul ignore next */
      {
        cov_1s4eda6hhm().b[7][1]++;
      }
      const {
        data: alerts,
        error
      } =
      /* istanbul ignore next */
      (cov_1s4eda6hhm().s[47]++, await query);
      /* istanbul ignore next */
      cov_1s4eda6hhm().s[48]++;
      if (error) {
        /* istanbul ignore next */
        cov_1s4eda6hhm().b[8][0]++;
        cov_1s4eda6hhm().s[49]++;
        _logger.logger.error('Failed to get alerts', {
          error
        });
        /* istanbul ignore next */
        cov_1s4eda6hhm().s[50]++;
        return (0, _apierrors.formatErrorResponse)('Failed to retrieve alerts', 500);
      } else
      /* istanbul ignore next */
      {
        cov_1s4eda6hhm().b[8][1]++;
      }
      // Calculate alert statistics
      const alertStats =
      /* istanbul ignore next */
      (cov_1s4eda6hhm().s[51]++,
      /* istanbul ignore next */
      (cov_1s4eda6hhm().b[9][0]++, alerts?.reduce((stats, alert) => {
        /* istanbul ignore next */
        cov_1s4eda6hhm().f[10]++;
        cov_1s4eda6hhm().s[52]++;
        stats.totalAlerts++;
        /* istanbul ignore next */
        cov_1s4eda6hhm().s[53]++;
        stats.bySeverity[alert.severity] = (
        /* istanbul ignore next */
        (cov_1s4eda6hhm().b[10][0]++, stats.bySeverity[alert.severity]) ||
        /* istanbul ignore next */
        (cov_1s4eda6hhm().b[10][1]++, 0)) + 1;
        /* istanbul ignore next */
        cov_1s4eda6hhm().s[54]++;
        stats.byType[alert.type] = (
        /* istanbul ignore next */
        (cov_1s4eda6hhm().b[11][0]++, stats.byType[alert.type]) ||
        /* istanbul ignore next */
        (cov_1s4eda6hhm().b[11][1]++, 0)) + 1;
        /* istanbul ignore next */
        cov_1s4eda6hhm().s[55]++;
        if (
        /* istanbul ignore next */
        (cov_1s4eda6hhm().b[13][0]++, alert.severity === 'critical') ||
        /* istanbul ignore next */
        (cov_1s4eda6hhm().b[13][1]++, alert.severity === 'high')) {
          /* istanbul ignore next */
          cov_1s4eda6hhm().b[12][0]++;
          cov_1s4eda6hhm().s[56]++;
          stats.criticalAlerts++;
        } else
        /* istanbul ignore next */
        {
          cov_1s4eda6hhm().b[12][1]++;
        }
        cov_1s4eda6hhm().s[57]++;
        return stats;
      }, {
        totalAlerts: 0,
        criticalAlerts: 0,
        bySeverity: {},
        byType: {}
      })) ||
      /* istanbul ignore next */
      (cov_1s4eda6hhm().b[9][1]++, {
        totalAlerts: 0,
        criticalAlerts: 0,
        bySeverity: {},
        byType: {}
      }));
      /* istanbul ignore next */
      cov_1s4eda6hhm().s[58]++;
      return _server.NextResponse.json({
        success: true,
        data: {
          alerts:
          /* istanbul ignore next */
          (cov_1s4eda6hhm().b[14][0]++, alerts) ||
          /* istanbul ignore next */
          (cov_1s4eda6hhm().b[14][1]++, []),
          statistics: alertStats,
          summary: {
            totalAlerts: alertStats.totalAlerts,
            criticalAlerts: alertStats.criticalAlerts,
            alertRate: alertStats.totalAlerts / hours,
            timeRange: `${hours} hours`
          }
        }
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_1s4eda6hhm().s[59]++;
      _logger.logger.error('Alerts retrieval error', {
        error
      });
      /* istanbul ignore next */
      cov_1s4eda6hhm().s[60]++;
      return (0, _apierrors.formatErrorResponse)('Internal server error', 500);
    }
  });
}
async function PATCH(request) {
  /* istanbul ignore next */
  cov_1s4eda6hhm().f[11]++;
  cov_1s4eda6hhm().s[61]++;
  return (0, _ratelimit.withRateLimit)(request, async () => {
    /* istanbul ignore next */
    cov_1s4eda6hhm().f[12]++;
    cov_1s4eda6hhm().s[62]++;
    try {
      const body =
      /* istanbul ignore next */
      (cov_1s4eda6hhm().s[63]++, await request.json());
      const {
        alertId,
        status
      } =
      /* istanbul ignore next */
      (cov_1s4eda6hhm().s[64]++, _zod.z.object({
        alertId: _zod.z.string(),
        status: _zod.z.enum(['active', 'acknowledged', 'resolved', 'dismissed'])
      }).parse(body));
      // Update alert status
      const supabase =
      /* istanbul ignore next */
      (cov_1s4eda6hhm().s[65]++, _supabase.supabaseAdmin);
      const {
        error
      } =
      /* istanbul ignore next */
      (cov_1s4eda6hhm().s[66]++, await supabase.from('monitoring_alerts').update({
        status,
        updated_at: new Date().toISOString()
      }).eq('id', alertId));
      /* istanbul ignore next */
      cov_1s4eda6hhm().s[67]++;
      if (error) {
        /* istanbul ignore next */
        cov_1s4eda6hhm().b[15][0]++;
        cov_1s4eda6hhm().s[68]++;
        _logger.logger.error('Failed to update alert status', {
          error
        });
        /* istanbul ignore next */
        cov_1s4eda6hhm().s[69]++;
        return (0, _apierrors.formatErrorResponse)('Failed to update alert', 500);
      } else
      /* istanbul ignore next */
      {
        cov_1s4eda6hhm().b[15][1]++;
      }
      cov_1s4eda6hhm().s[70]++;
      _logger.logger.info('Alert status updated', {
        alertId,
        status
      });
      /* istanbul ignore next */
      cov_1s4eda6hhm().s[71]++;
      return _server.NextResponse.json({
        success: true,
        message: 'Alert status updated successfully'
      });
    } catch (error) {
      /* istanbul ignore next */
      cov_1s4eda6hhm().s[72]++;
      _logger.logger.error('Alert status update error', {
        error
      });
      /* istanbul ignore next */
      cov_1s4eda6hhm().s[73]++;
      return (0, _apierrors.formatErrorResponse)('Invalid request', 400);
    }
  });
}
// Send critical alerts via various channels
async function sendCriticalAlerts(alerts) {
  /* istanbul ignore next */
  cov_1s4eda6hhm().f[13]++;
  cov_1s4eda6hhm().s[74]++;
  try {
    /* istanbul ignore next */
    cov_1s4eda6hhm().s[75]++;
    // Send to Slack/Teams/Discord
    await sendToSlack(alerts);
    // Send email alerts
    /* istanbul ignore next */
    cov_1s4eda6hhm().s[76]++;
    await sendEmailAlerts(alerts);
    // Send SMS alerts (if configured)
    /* istanbul ignore next */
    cov_1s4eda6hhm().s[77]++;
    await sendSMSAlerts(alerts);
    /* istanbul ignore next */
    cov_1s4eda6hhm().s[78]++;
    _logger.logger.info('Critical alerts sent', {
      count: alerts.length
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_1s4eda6hhm().s[79]++;
    _logger.logger.error('Failed to send critical alerts', {
      error
    });
  }
}
async function sendToSlack(alerts) {
  /* istanbul ignore next */
  cov_1s4eda6hhm().f[14]++;
  const webhookUrl =
  /* istanbul ignore next */
  (cov_1s4eda6hhm().s[80]++, process.env.SLACK_WEBHOOK_URL);
  /* istanbul ignore next */
  cov_1s4eda6hhm().s[81]++;
  if (!webhookUrl) {
    /* istanbul ignore next */
    cov_1s4eda6hhm().b[16][0]++;
    cov_1s4eda6hhm().s[82]++;
    return;
  } else
  /* istanbul ignore next */
  {
    cov_1s4eda6hhm().b[16][1]++;
  }
  cov_1s4eda6hhm().s[83]++;
  try {
    const blocks =
    /* istanbul ignore next */
    (cov_1s4eda6hhm().s[84]++, alerts.map(alert => {
      /* istanbul ignore next */
      cov_1s4eda6hhm().f[15]++;
      cov_1s4eda6hhm().s[85]++;
      return {
        type: 'section',
        text: {
          type: 'mrkdwn',
          text: ` *${alert.type.toUpperCase()}*\n${alert.message}\n*Severity:* ${alert.severity}\n*Time:* ${new Date(alert.timestamp).toLocaleString()}`
        }
      };
    }));
    /* istanbul ignore next */
    cov_1s4eda6hhm().s[86]++;
    await fetch(webhookUrl, {
      method: 'POST',
      headers: {
        'Content-Type': 'application/json'
      },
      body: JSON.stringify({
        text: `Critical Alert: ${alerts.length} new critical alerts`,
        blocks
      })
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_1s4eda6hhm().s[87]++;
    _logger.logger.error('Failed to send to Slack', {
      error
    });
  }
}
