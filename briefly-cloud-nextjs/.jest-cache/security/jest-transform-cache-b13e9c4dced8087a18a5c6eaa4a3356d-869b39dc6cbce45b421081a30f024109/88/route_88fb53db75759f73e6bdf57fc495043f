/**
 * Validate file content for security
 */
async function validateFileContent(file) {
  /* istanbul ignore next */
  cov_1s72wdcmbz().f[5]++;
  cov_1s72wdcmbz().s[63]++;
  try {
    // Check file signature/magic bytes
    const buffer =
    /* istanbul ignore next */
    (cov_1s72wdcmbz().s[64]++, await file.slice(0, 512).arrayBuffer());
    const bytes =
    /* istanbul ignore next */
    (cov_1s72wdcmbz().s[65]++, new Uint8Array(buffer));
    // Basic file type validation based on magic bytes
    const fileSignatures =
    /* istanbul ignore next */
    (cov_1s72wdcmbz().s[66]++, {
      'application/pdf': [[0x25, 0x50, 0x44, 0x46]],
      'application/vnd.openxmlformats-officedocument.wordprocessingml.document': [[0x50, 0x4B, 0x03, 0x04], [0x50, 0x4B, 0x05, 0x06], [0x50, 0x4B, 0x07, 0x08]],
      'text/plain': [],
      'text/markdown': []
    });
    const expectedSignatures =
    /* istanbul ignore next */
    (cov_1s72wdcmbz().s[67]++, fileSignatures[file.type]);
    /* istanbul ignore next */
    cov_1s72wdcmbz().s[68]++;
    if (
    /* istanbul ignore next */
    (cov_1s72wdcmbz().b[24][0]++, expectedSignatures) &&
    /* istanbul ignore next */
    (cov_1s72wdcmbz().b[24][1]++, expectedSignatures.length > 0)) {
      /* istanbul ignore next */
      cov_1s72wdcmbz().b[23][0]++;
      const hasValidSignature =
      /* istanbul ignore next */
      (cov_1s72wdcmbz().s[69]++, expectedSignatures.some(signature => {
        /* istanbul ignore next */
        cov_1s72wdcmbz().f[6]++;
        cov_1s72wdcmbz().s[70]++;
        return signature.every((byte, index) => {
          /* istanbul ignore next */
          cov_1s72wdcmbz().f[7]++;
          cov_1s72wdcmbz().s[71]++;
          return bytes[index] === byte;
        });
      }));
      /* istanbul ignore next */
      cov_1s72wdcmbz().s[72]++;
      if (!hasValidSignature) {
        /* istanbul ignore next */
        cov_1s72wdcmbz().b[25][0]++;
        cov_1s72wdcmbz().s[73]++;
        return {
          valid: false,
          error: 'File content does not match declared type'
        };
      } else
      /* istanbul ignore next */
      {
        cov_1s72wdcmbz().b[25][1]++;
      }
    } else
    /* istanbul ignore next */
    {
      cov_1s72wdcmbz().b[23][1]++;
    }
    // Check for suspicious content patterns
    const suspiciousPatterns =
    /* istanbul ignore next */
    (cov_1s72wdcmbz().s[74]++, [/javascript:/gi, /<script/gi, /eval\(/gi, /document\.write/gi, /window\.location/gi]);
    // Convert first 1KB to string for pattern checking
    const textContent =
    /* istanbul ignore next */
    (cov_1s72wdcmbz().s[75]++, new TextDecoder('utf-8', {
      fatal: false
    }).decode(bytes.slice(0, 1024)));
    const hasSuspiciousContent =
    /* istanbul ignore next */
    (cov_1s72wdcmbz().s[76]++, suspiciousPatterns.some(pattern => {
      /* istanbul ignore next */
      cov_1s72wdcmbz().f[8]++;
      cov_1s72wdcmbz().s[77]++;
      return pattern.test(textContent);
    }));
    /* istanbul ignore next */
    cov_1s72wdcmbz().s[78]++;
    if (hasSuspiciousContent) {
      /* istanbul ignore next */
      cov_1s72wdcmbz().b[26][0]++;
      cov_1s72wdcmbz().s[79]++;
      return {
        valid: false,
        error: 'File contains suspicious content'
      };
    } else
    /* istanbul ignore next */
    {
      cov_1s72wdcmbz().b[26][1]++;
    }
    cov_1s72wdcmbz().s[80]++;
    return {
      valid: true
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_1s72wdcmbz().s[81]++;
    _logger.logger.error('File validation error', {
      fileName: file.name
    }, error);
    /* istanbul ignore next */
    cov_1s72wdcmbz().s[82]++;
    return {
      valid: false,
      error: 'File validation failed'
