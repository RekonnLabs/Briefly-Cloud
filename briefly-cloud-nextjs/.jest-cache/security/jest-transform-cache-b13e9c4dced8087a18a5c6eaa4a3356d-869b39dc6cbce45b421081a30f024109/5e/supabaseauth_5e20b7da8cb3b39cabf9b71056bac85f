async function createOrUpdateUserProfile(userId, email, fullName, avatarUrl) {
  /* istanbul ignore next */
  cov_1ntv6zjbz0().f[27]++;
  // Check if user exists
  const {
    data: existingUser
  } =
  /* istanbul ignore next */
  (cov_1ntv6zjbz0().s[62]++, await _supabase.supabaseAdmin.from('app.users').select('id').eq('id', userId).single());
  /* istanbul ignore next */
  cov_1ntv6zjbz0().s[63]++;
  if (!existingUser) {
    /* istanbul ignore next */
    cov_1ntv6zjbz0().b[8][0]++;
    // Create new user with free tier defaults
    const newUser =
    /* istanbul ignore next */
    (cov_1ntv6zjbz0().s[64]++, {
      id: userId,
      email,
      full_name:
      /* istanbul ignore next */
      (cov_1ntv6zjbz0().b[9][0]++, fullName) ||
      /* istanbul ignore next */
      (cov_1ntv6zjbz0().b[9][1]++, email.split('@')[0]),
      avatar_url: avatarUrl,
      subscription_tier: 'free',
      subscription_status: 'active',
      chat_messages_count: 0,
      chat_messages_limit: TIER_LIMITS.free.max_llm_calls,
      documents_uploaded: 0,
      documents_limit: TIER_LIMITS.free.max_files,
      api_calls_count: 0,
      api_calls_limit: 1000,
      storage_used_bytes: 0,
      storage_limit_bytes: TIER_LIMITS.free.max_storage_bytes,
      usage_stats: {},
      preferences: {},
      features_enabled: {
        cloud_storage: true,
        ai_chat: true,
        document_upload: true
      },
      permissions: {
        can_upload: true,
        can_chat: true,
        can_export: false
      },
      usage_reset_date: new Date(Date.now() + 30 * 24 * 60 * 60 * 1000).toISOString(),
      trial_end_date: new Date(Date.now() + 7 * 24 * 60 * 60 * 1000).toISOString(),
      last_login_at: new Date().toISOString(),
      gdpr_consent_version: '1.0',
      marketing_consent: false,
      analytics_consent: true
    });
    const {
      error
    } =
    /* istanbul ignore next */
    (cov_1ntv6zjbz0().s[65]++, await _supabase.supabaseAdmin.from('app.users').insert(newUser));
    /* istanbul ignore next */
    cov_1ntv6zjbz0().s[66]++;
    if (error) {
      /* istanbul ignore next */
      cov_1ntv6zjbz0().b[10][0]++;
      cov_1ntv6zjbz0().s[67]++;
      throw new Error(`Failed to create user profile: ${error.message}`);
    } else
    /* istanbul ignore next */
    {
      cov_1ntv6zjbz0().b[10][1]++;
    }
    cov_1ntv6zjbz0().s[68]++;
    return {
      id: userId,
      email,
      full_name: newUser.full_name,
      avatar_url: avatarUrl,
      subscription_tier: 'free',
      subscription_status: 'active',
      usage_count: 0,
      usage_limit: TIER_LIMITS.free.max_llm_calls,
      features_enabled: newUser.features_enabled,
      permissions: newUser.permissions,
      last_login_at: newUser.last_login_at,
      created_at: new Date().toISOString()
    };
  } else {
    /* istanbul ignore next */
    cov_1ntv6zjbz0().b[8][1]++;
    // Update existing user's login timestamp and profile info
    const {
      data: updatedUser,
      error
    } =
    /* istanbul ignore next */
    (cov_1ntv6zjbz0().s[69]++, await _supabase.supabaseAdmin.from('app.users').update({
      full_name:
      /* istanbul ignore next */
      (cov_1ntv6zjbz0().b[11][0]++, fullName) ||
      /* istanbul ignore next */
      (cov_1ntv6zjbz0().b[11][1]++, undefined),
      avatar_url:
      /* istanbul ignore next */
      (cov_1ntv6zjbz0().b[12][0]++, avatarUrl) ||
      /* istanbul ignore next */
      (cov_1ntv6zjbz0().b[12][1]++, undefined),
      last_login_at: new Date().toISOString()
    }).eq('id', userId).select(`
        id,
        email,
        full_name,
        avatar_url,
        subscription_tier,
        subscription_status,
        chat_messages_count,
        chat_messages_limit,
        features_enabled,
        permissions,
        last_login_at,
        created_at
      `).single());
    /* istanbul ignore next */
    cov_1ntv6zjbz0().s[70]++;
    if (
    /* istanbul ignore next */
