    cov_1ntv6zjbz0().b[2][1]++;
  }
  // Update last login timestamp
  cov_1ntv6zjbz0().s[47]++;
  await _supabase.supabaseAdmin.from('app.users').update({
    last_login_at: new Date().toISOString()
  }).eq('id', user.id);
  /* istanbul ignore next */
  cov_1ntv6zjbz0().s[48]++;
