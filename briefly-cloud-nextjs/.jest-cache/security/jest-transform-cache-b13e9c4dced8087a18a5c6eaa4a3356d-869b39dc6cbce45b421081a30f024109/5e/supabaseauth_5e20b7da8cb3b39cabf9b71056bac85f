    cov_1ntv6zjbz0().b[0][1]++;
  }
  // Get full user profile from app.users table
  const {
    data: userProfile,
    error: profileError
  } =
  /* istanbul ignore next */
  (cov_1ntv6zjbz0().s[44]++, await _supabase.supabaseAdmin.from('app.users').select(`
      id,
      email,
      full_name,
      avatar_url,
      subscription_tier,
      subscription_status,
      chat_messages_count,
