    /* istanbul ignore next */
    (cov_1ntv6zjbz0().b[4][1]++, 0),
    usage_limit:
    /* istanbul ignore next */
    (cov_1ntv6zjbz0().b[5][0]++, userProfile.chat_messages_limit) ||
    /* istanbul ignore next */
    (cov_1ntv6zjbz0().b[5][1]++, TIER_LIMITS[userProfile.subscription_tier]?.max_llm_calls) ||
