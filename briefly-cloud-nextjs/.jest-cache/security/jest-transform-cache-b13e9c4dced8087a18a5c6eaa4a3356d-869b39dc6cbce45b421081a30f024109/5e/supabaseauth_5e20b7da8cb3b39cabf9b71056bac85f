async function validateSession() {
  /* istanbul ignore next */
  cov_1ntv6zjbz0().f[28]++;
  cov_1ntv6zjbz0().s[73]++;
  try {
    const supabase =
    /* istanbul ignore next */
    (cov_1ntv6zjbz0().s[74]++, createSupabaseServerClient());
    const {
      data: {
        session
      },
      error
    } =
    /* istanbul ignore next */
    (cov_1ntv6zjbz0().s[75]++, await supabase.auth.getSession());
    /* istanbul ignore next */
    cov_1ntv6zjbz0().s[76]++;
    if (
    /* istanbul ignore next */
    (cov_1ntv6zjbz0().b[20][0]++, error) ||
    /* istanbul ignore next */
    (cov_1ntv6zjbz0().b[20][1]++, !session)) {
      /* istanbul ignore next */
      cov_1ntv6zjbz0().b[19][0]++;
      cov_1ntv6zjbz0().s[77]++;
      return {
        valid: false
      };
    } else
    /* istanbul ignore next */
    {
      cov_1ntv6zjbz0().b[19][1]++;
    }
    // Check if session is expired
    cov_1ntv6zjbz0().s[78]++;
    if (
    /* istanbul ignore next */
    (cov_1ntv6zjbz0().b[22][0]++, session.expires_at) &&
    /* istanbul ignore next */
    (cov_1ntv6zjbz0().b[22][1]++, session.expires_at * 1000 < Date.now())) {
      /* istanbul ignore next */
      cov_1ntv6zjbz0().b[21][0]++;
      cov_1ntv6zjbz0().s[79]++;
      return {
        valid: false
      };
    } else
    /* istanbul ignore next */
    {
      cov_1ntv6zjbz0().b[21][1]++;
    }
    const user =
    /* istanbul ignore next */
    (cov_1ntv6zjbz0().s[80]++, await getAuthenticatedUser());
    /* istanbul ignore next */
    cov_1ntv6zjbz0().s[81]++;
    return {
      valid: true,
      user
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_1ntv6zjbz0().s[82]++;
    return {
      valid: false
    };
  }
}
function getOAuthConfig() {
  /* istanbul ignore next */
  cov_1ntv6zjbz0().f[29]++;
  cov_1ntv6zjbz0().s[83]++;
  return {
    google: {
      enabled: !!(
      /* istanbul ignore next */
      (cov_1ntv6zjbz0().b[23][0]++, process.env.GOOGLE_CLIENT_ID) &&
      /* istanbul ignore next */
      (cov_1ntv6zjbz0().b[23][1]++, process.env.GOOGLE_CLIENT_SECRET)),
      scopes: 'openid email profile'
    },
    microsoft: {
      enabled: !!(
      /* istanbul ignore next */
      (cov_1ntv6zjbz0().b[24][0]++, process.env.AZURE_AD_CLIENT_ID) &&
      /* istanbul ignore next */
      (cov_1ntv6zjbz0().b[24][1]++, process.env.AZURE_AD_CLIENT_SECRET)),
      scopes: 'openid email profile'
    }
  };
}
