}
function securityHeadersMiddleware(handler) {
  /* istanbul ignore next */
  cov_15tlf38p3g().f[19]++;
  cov_15tlf38p3g().s[96]++;
  return async request => {
    /* istanbul ignore next */
    cov_15tlf38p3g().f[20]++;
    cov_15tlf38p3g().s[97]++;
    try {
      // Execute the handler
      const response =
      /* istanbul ignore next */
      (cov_15tlf38p3g().s[98]++, await handler(request));
      // Apply security headers to the response
      /* istanbul ignore next */
      cov_15tlf38p3g().s[99]++;
      return applySecurityHeaders(response);
    } catch (error) {
      /* istanbul ignore next */
      cov_15tlf38p3g().s[100]++;
      _logger.logger.error('Security headers middleware error', error);
      // Create error response with security headers
      const errorResponse =
      /* istanbul ignore next */
      (cov_15tlf38p3g().s[101]++, _server.NextResponse.json({
        error: 'Internal server error'
      }, {
        status: 500
      }));
      /* istanbul ignore next */
      cov_15tlf38p3g().s[102]++;
      return applySecurityHeaders(errorResponse);
    }
  };
}
function validateSecurityHeaders(response) {
  /* istanbul ignore next */
  cov_15tlf38p3g().f[21]++;
  const requiredHeaders =
  /* istanbul ignore next */
  (cov_15tlf38p3g().s[103]++, ['X-Content-Type-Options', 'X-Frame-Options', 'X-XSS-Protection', 'Referrer-Policy', 'Permissions-Policy']);
  const productionHeaders =
  /* istanbul ignore next */
  (cov_15tlf38p3g().s[104]++, ['Strict-Transport-Security', 'Content-Security-Policy']);
  const missing =
  /* istanbul ignore next */
  (cov_15tlf38p3g().s[105]++, []);
  const warnings =
  /* istanbul ignore next */
  (cov_15tlf38p3g().s[106]++, []);
  // Check required headers
  /* istanbul ignore next */
  cov_15tlf38p3g().s[107]++;
  requiredHeaders.forEach(header => {
    /* istanbul ignore next */
    cov_15tlf38p3g().f[22]++;
    cov_15tlf38p3g().s[108]++;
    if (!response.headers.get(header)) {
      /* istanbul ignore next */
      cov_15tlf38p3g().b[36][0]++;
      cov_15tlf38p3g().s[109]++;
      missing.push(header);
    } else
    /* istanbul ignore next */
    {
      cov_15tlf38p3g().b[36][1]++;
    }
  });
  // Check production-specific headers
  /* istanbul ignore next */
  cov_15tlf38p3g().s[110]++;
  if ((0, _environment.isProduction)()) {
    /* istanbul ignore next */
    cov_15tlf38p3g().b[37][0]++;
    cov_15tlf38p3g().s[111]++;
    productionHeaders.forEach(header => {
      /* istanbul ignore next */
      cov_15tlf38p3g().f[23]++;
      cov_15tlf38p3g().s[112]++;
      if (!response.headers.get(header)) {
        /* istanbul ignore next */
        cov_15tlf38p3g().b[38][0]++;
        cov_15tlf38p3g().s[113]++;
        warnings.push(`Missing production header: ${header}`);
      } else
      /* istanbul ignore next */
      {
        cov_15tlf38p3g().b[38][1]++;
      }
    });
  } else
  /* istanbul ignore next */
  {
    cov_15tlf38p3g().b[37][1]++;
  }
  // Validate CSP
  const csp =
  /* istanbul ignore next */
  (cov_15tlf38p3g().s[114]++, response.headers.get('Content-Security-Policy'));
  /* istanbul ignore next */
  cov_15tlf38p3g().s[115]++;
  if (csp) {
    /* istanbul ignore next */
    cov_15tlf38p3g().b[39][0]++;
    cov_15tlf38p3g().s[116]++;
    if (
    /* istanbul ignore next */
    (cov_15tlf38p3g().b[41][0]++, csp.includes("'unsafe-eval'")) &&
    /* istanbul ignore next */
    (cov_15tlf38p3g().b[41][1]++, (0, _environment.isProduction)())) {
      /* istanbul ignore next */
      cov_15tlf38p3g().b[40][0]++;
      cov_15tlf38p3g().s[117]++;
      warnings.push("CSP contains 'unsafe-eval' in production");
    } else
    /* istanbul ignore next */
    {
      cov_15tlf38p3g().b[40][1]++;
    }
    cov_15tlf38p3g().s[118]++;
    if (
    /* istanbul ignore next */
    (cov_15tlf38p3g().b[43][0]++, csp.includes("'unsafe-inline'")) &&
    /* istanbul ignore next */
    (cov_15tlf38p3g().b[43][1]++, (0, _environment.isProduction)())) {
      /* istanbul ignore next */
      cov_15tlf38p3g().b[42][0]++;
      cov_15tlf38p3g().s[119]++;
      warnings.push("CSP contains 'unsafe-inline' in production");
    } else
    /* istanbul ignore next */
    {
      cov_15tlf38p3g().b[42][1]++;
    }
  } else
  /* istanbul ignore next */
  {
    cov_15tlf38p3g().b[39][1]++;
  }
  cov_15tlf38p3g().s[120]++;
  return {
    valid: missing.length === 0,
    missing,
    warnings
  };
}
