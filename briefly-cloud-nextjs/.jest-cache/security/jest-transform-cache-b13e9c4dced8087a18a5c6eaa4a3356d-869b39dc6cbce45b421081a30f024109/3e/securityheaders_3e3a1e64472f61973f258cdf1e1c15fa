      cov_15tlf38p3g().b[18][1]++;
    }
    // Referrer Policy
    cov_15tlf38p3g().s[36]++;
    if (enableReferrerPolicy) {
      /* istanbul ignore next */
      cov_15tlf38p3g().b[19][0]++;
      cov_15tlf38p3g().s[37]++;
      response.headers.set('Referrer-Policy', 'strict-origin-when-cross-origin');
    } else
    /* istanbul ignore next */
    {
      cov_15tlf38p3g().b[19][1]++;
    }
    // Permissions Policy
    cov_15tlf38p3g().s[38]++;
    if (enablePermissionsPolicy) {
      /* istanbul ignore next */
      cov_15tlf38p3g().b[20][0]++;
      const permissionsPolicy =
      /* istanbul ignore next */
      (cov_15tlf38p3g().s[39]++, ['camera=()', 'microphone=()', 'geolocation=()', 'interest-cohort=()', 'payment=()', 'usb=()', 'bluetooth=()', 'magnetometer=()', 'gyroscope=()', 'accelerometer=()'].join(', '));
      /* istanbul ignore next */
      cov_15tlf38p3g().s[40]++;
      response.headers.set('Permissions-Policy', permissionsPolicy);
    } else
    /* istanbul ignore next */
    {
      cov_15tlf38p3g().b[20][1]++;
    }
    // CORS Headers (if enabled)
    cov_15tlf38p3g().s[41]++;
    if (enableCORS) {
      /* istanbul ignore next */
      cov_15tlf38p3g().b[21][0]++;
      cov_15tlf38p3g().s[42]++;
      applyCORSHeaders(response, securityConfig.cors.origins);
    } else
    /* istanbul ignore next */
    {
      cov_15tlf38p3g().b[21][1]++;
    }
    // Additional security headers
    cov_15tlf38p3g().s[43]++;
    response.headers.set('X-DNS-Prefetch-Control', 'off');
    /* istanbul ignore next */
    cov_15tlf38p3g().s[44]++;
    response.headers.set('X-Download-Options', 'noopen');
    /* istanbul ignore next */
    cov_15tlf38p3g().s[45]++;
    response.headers.set('X-Permitted-Cross-Domain-Policies', 'none');
    /* istanbul ignore next */
    cov_15tlf38p3g().s[46]++;
    response.headers.set('Cross-Origin-Embedder-Policy', 'require-corp');
    /* istanbul ignore next */
    cov_15tlf38p3g().s[47]++;
    response.headers.set('Cross-Origin-Opener-Policy', 'same-origin');
    /* istanbul ignore next */
    cov_15tlf38p3g().s[48]++;
    response.headers.set('Cross-Origin-Resource-Policy', 'same-origin');
    // Custom headers
    /* istanbul ignore next */
    cov_15tlf38p3g().s[49]++;
    Object.entries(customHeaders).forEach(([name, value]) => {
      /* istanbul ignore next */
      cov_15tlf38p3g().f[10]++;
      cov_15tlf38p3g().s[50]++;
      response.headers.set(name, value);
    });
    // Security metadata headers
    /* istanbul ignore next */
    cov_15tlf38p3g().s[51]++;
    response.headers.set('X-Security-Headers', 'applied');
    /* istanbul ignore next */
    cov_15tlf38p3g().s[52]++;
    response.headers.set('X-Content-Security-Policy-Report-Only', 'false');
    /* istanbul ignore next */
    cov_15tlf38p3g().s[53]++;
    if ((0, _environment.isProduction)()) {
      /* istanbul ignore next */
      cov_15tlf38p3g().b[22][0]++;
      cov_15tlf38p3g().s[54]++;
      response.headers.set('Server', 'Briefly-Cloud');
      /* istanbul ignore next */
      cov_15tlf38p3g().s[55]++;
      response.headers.delete('X-Powered-By');
    } else
    /* istanbul ignore next */
    {
      cov_15tlf38p3g().b[22][1]++;
    }
    cov_15tlf38p3g().s[56]++;
    return response;
  } catch (error) {
    /* istanbul ignore next */
    cov_15tlf38p3g().s[57]++;
    _logger.logger.error('Failed to apply security headers', error);
    /* istanbul ignore next */
    cov_15tlf38p3g().s[58]++;
    return response;
  }
}
/**
 * Apply CORS headers based on origin validation
 */
function applyCORSHeaders(response, allowedOrigins) {
  /* istanbul ignore next */
  cov_15tlf38p3g().f[11]++;
  cov_15tlf38p3g().s[59]++;
  // Note: In middleware, we don't have access to the request origin
  // This is typically handled in API routes or middleware.ts
  response.headers.set('Access-Control-Allow-Methods', 'GET, POST, PUT, DELETE, OPTIONS');
  /* istanbul ignore next */
  cov_15tlf38p3g().s[60]++;
  response.headers.set('Access-Control-Allow-Headers', 'Content-Type, Authorization, X-Requested-With');
  /* istanbul ignore next */
  cov_15tlf38p3g().s[61]++;
  response.headers.set('Access-Control-Max-Age', '86400'); // 24 hours
}
function validateCORSOrigin(origin, allowedOrigins) {
  /* istanbul ignore next */
  cov_15tlf38p3g().f[12]++;
  cov_15tlf38p3g().s[62]++;
  if (!origin) {
    /* istanbul ignore next */
    cov_15tlf38p3g().b[23][0]++;
    cov_15tlf38p3g().s[63]++;
    return false;
  } else
  /* istanbul ignore next */
  {
    cov_15tlf38p3g().b[23][1]++;
  }
  // Exact match
  cov_15tlf38p3g().s[64]++;
  if (allowedOrigins.includes(origin)) {
    /* istanbul ignore next */
    cov_15tlf38p3g().b[24][0]++;
    cov_15tlf38p3g().s[65]++;
    return true;
  } else
  /* istanbul ignore next */
  {
    cov_15tlf38p3g().b[24][1]++;
  }
  // Wildcard matching for development
  cov_15tlf38p3g().s[66]++;
  if (!(0, _environment.isProduction)()) {
    /* istanbul ignore next */
    cov_15tlf38p3g().b[25][0]++;
    cov_15tlf38p3g().s[67]++;
    return allowedOrigins.some(allowed => {
      /* istanbul ignore next */
      cov_15tlf38p3g().f[13]++;
      cov_15tlf38p3g().s[68]++;
      if (allowed.includes('*')) {
        /* istanbul ignore next */
        cov_15tlf38p3g().b[26][0]++;
        const pattern =
        /* istanbul ignore next */
        (cov_15tlf38p3g().s[69]++, allowed.replace(/\*/g, '.*'));
        /* istanbul ignore next */
        cov_15tlf38p3g().s[70]++;
        return new RegExp(`^${pattern}$`).test(origin);
      } else
      /* istanbul ignore next */
      {
        cov_15tlf38p3g().b[26][1]++;
      }
      cov_15tlf38p3g().s[71]++;
      return false;
    });
  } else
  /* istanbul ignore next */
  {
    cov_15tlf38p3g().b[25][1]++;
  }
  cov_15tlf38p3g().s[72]++;
  return false;
}
function createCORSMiddleware(allowedOrigins) {
  /* istanbul ignore next */
  cov_15tlf38p3g().f[14]++;
  cov_15tlf38p3g().s[73]++;
  return (request, response) => {
    /* istanbul ignore next */
    cov_15tlf38p3g().f[15]++;
    const securityConfig =
    /* istanbul ignore next */
    (cov_15tlf38p3g().s[74]++, (0, _environment.getSecurityConfig)());
    const origins =
    /* istanbul ignore next */
    (cov_15tlf38p3g().s[75]++,
    /* istanbul ignore next */
    (cov_15tlf38p3g().b[27][0]++, allowedOrigins) ||
    /* istanbul ignore next */
    (cov_15tlf38p3g().b[27][1]++, securityConfig.cors.origins));
    const origin =
    /* istanbul ignore next */
    (cov_15tlf38p3g().s[76]++, request.headers.get('origin'));
    // Validate origin
    /* istanbul ignore next */
    cov_15tlf38p3g().s[77]++;
    if (
    /* istanbul ignore next */
    (cov_15tlf38p3g().b[29][0]++, origin) &&
    /* istanbul ignore next */
    (cov_15tlf38p3g().b[29][1]++, validateCORSOrigin(origin, origins))) {
      /* istanbul ignore next */
      cov_15tlf38p3g().b[28][0]++;
      cov_15tlf38p3g().s[78]++;
      response.headers.set('Access-Control-Allow-Origin', origin);
      /* istanbul ignore next */
      cov_15tlf38p3g().s[79]++;
      response.headers.set('Access-Control-Allow-Credentials', 'true');
    } else
    /* istanbul ignore next */
    {
      cov_15tlf38p3g().b[28][1]++;
    }
    // Handle preflight requests
    cov_15tlf38p3g().s[80]++;
    if (request.method === 'OPTIONS') {
      /* istanbul ignore next */
      cov_15tlf38p3g().b[30][0]++;
      cov_15tlf38p3g().s[81]++;
      response.headers.set('Access-Control-Allow-Methods', securityConfig.cors.methods.join(', '));
      /* istanbul ignore next */
      cov_15tlf38p3g().s[82]++;
      response.headers.set('Access-Control-Allow-Headers', securityConfig.cors.allowedHeaders.join(', '));
      /* istanbul ignore next */
      cov_15tlf38p3g().s[83]++;
      response.headers.set('Access-Control-Max-Age', '86400');
      /* istanbul ignore next */
      cov_15tlf38p3g().s[84]++;
      return new _server.NextResponse(null, {
        status: 200
      });
    } else
    /* istanbul ignore next */
    {
      cov_15tlf38p3g().b[30][1]++;
    }
    cov_15tlf38p3g().s[85]++;
    return response;
  };
}
const CONTENT_TYPE_HEADERS =
/* istanbul ignore next */
(cov_15tlf38p3g().s[86]++, {
  json: {
    'Content-Type': 'application/json; charset=utf-8',
    'X-Content-Type-Options': 'nosniff'
  },
  html: {
    'Content-Type': 'text/html; charset=utf-8',
    'X-Content-Type-Options': 'nosniff',
    'X-Frame-Options': 'DENY'
  },
  css: {
    'Content-Type': 'text/css; charset=utf-8',
    'X-Content-Type-Options': 'nosniff'
  },
  javascript: {
    'Content-Type': 'application/javascript; charset=utf-8',
    'X-Content-Type-Options': 'nosniff'
  }
});
function applyContentTypeHeaders(response, contentType) {
  /* istanbul ignore next */
  cov_15tlf38p3g().f[16]++;
  const headers =
  /* istanbul ignore next */
  (cov_15tlf38p3g().s[87]++, CONTENT_TYPE_HEADERS[contentType]);
  /* istanbul ignore next */
  cov_15tlf38p3g().s[88]++;
  Object.entries(headers).forEach(([name, value]) => {
    /* istanbul ignore next */
    cov_15tlf38p3g().f[17]++;
    cov_15tlf38p3g().s[89]++;
    response.headers.set(name, value);
  });
  /* istanbul ignore next */
  cov_15tlf38p3g().s[90]++;
  return response;
}
function createSecureResponse(data, options =
/* istanbul ignore next */
(cov_15tlf38p3g().b[31][0]++, {})) {
  /* istanbul ignore next */
  cov_15tlf38p3g().f[18]++;
  const {
    status =
    /* istanbul ignore next */
    (cov_15tlf38p3g().b[32][0]++, 200),
    headers =
    /* istanbul ignore next */
    (cov_15tlf38p3g().b[33][0]++, {}),
    contentType =
    /* istanbul ignore next */
    (cov_15tlf38p3g().b[34][0]++, 'json'),
    securityConfig =
    /* istanbul ignore next */
    (cov_15tlf38p3g().b[35][0]++, {})
  } =
  /* istanbul ignore next */
  (cov_15tlf38p3g().s[91]++, options);
  // Create response
  const response =
  /* istanbul ignore next */
  (cov_15tlf38p3g().s[92]++, _server.NextResponse.json(data, {
    status,
    headers
  }));
  // Apply content-type headers
  /* istanbul ignore next */
  cov_15tlf38p3g().s[93]++;
  applyContentTypeHeaders(response, contentType);
  // Apply security headers
  /* istanbul ignore next */
  cov_15tlf38p3g().s[94]++;
  applySecurityHeaders(response, securityConfig);
  /* istanbul ignore next */
  cov_15tlf38p3g().s[95]++;
