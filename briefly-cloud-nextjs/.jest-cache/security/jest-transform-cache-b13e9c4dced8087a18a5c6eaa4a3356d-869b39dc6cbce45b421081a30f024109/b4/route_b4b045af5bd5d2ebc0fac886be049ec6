    /* istanbul ignore next */
    {
      cov_1v200j0q4j().b[6][1]++;
    }
    const secureStorage =
    /* istanbul ignore next */
    (cov_1v200j0q4j().s[39]++, (0, _securestorage.getSecureStorage)());
    const results =
    /* istanbul ignore next */
    (cov_1v200j0q4j().s[40]++, []);
    let successCount =
    /* istanbul ignore next */
    (cov_1v200j0q4j().s[41]++, 0);
    let errorCount =
    /* istanbul ignore next */
    (cov_1v200j0q4j().s[42]++, 0);
    // Process deletions
    /* istanbul ignore next */
    cov_1v200j0q4j().s[43]++;
    for (const fileId of fileIds) {
      /* istanbul ignore next */
      cov_1v200j0q4j().s[44]++;
      try {
        const result =
        /* istanbul ignore next */
        (cov_1v200j0q4j().s[45]++, await secureStorage.deleteFile(fileId, user.id,
        /* istanbul ignore next */
        (cov_1v200j0q4j().b[7][0]++, request.headers.get('x-forwarded-for')) ||
        /* istanbul ignore next */
        (cov_1v200j0q4j().b[7][1]++, 'unknown'),
        /* istanbul ignore next */
        (cov_1v200j0q4j().b[8][0]++, request.headers.get('user-agent')) ||
        /* istanbul ignore next */
        (cov_1v200j0q4j().b[8][1]++, 'unknown')));
        /* istanbul ignore next */
        cov_1v200j0q4j().s[46]++;
        if (result.success) {
          /* istanbul ignore next */
          cov_1v200j0q4j().b[9][0]++;
          cov_1v200j0q4j().s[47]++;
          successCount++;
          /* istanbul ignore next */
          cov_1v200j0q4j().s[48]++;
          results.push({
            fileId,
            success: true
          });
        } else {
          /* istanbul ignore next */
          cov_1v200j0q4j().b[9][1]++;
          cov_1v200j0q4j().s[49]++;
          errorCount++;
          /* istanbul ignore next */
          cov_1v200j0q4j().s[50]++;
          results.push({
            fileId,
            success: false,
            error: result.error
          });
        }
      } catch (error) {
        /* istanbul ignore next */
        cov_1v200j0q4j().s[51]++;
        errorCount++;
        /* istanbul ignore next */
        cov_1v200j0q4j().s[52]++;
        results.push({
          fileId,
          success: false,
          error: error.message
        });
      }
    }
    /* istanbul ignore next */
    cov_1v200j0q4j().s[53]++;
    return _server.NextResponse.json({
      success: successCount > 0,
      message: `${successCount} files deleted successfully${errorCount > 0 ?
      /* istanbul ignore next */
      (cov_1v200j0q4j().b[10][0]++, `, ${errorCount} failed`) :
      /* istanbul ignore next */
      (cov_1v200j0q4j().b[10][1]++, '')}`,
