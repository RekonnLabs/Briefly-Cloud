        results,
        summary: {
          total: fileIds.length,
          successful: successCount,
          failed: errorCount
        }
      }
    });
  } catch (error) {
    /* istanbul ignore next */
    cov_1v200j0q4j().s[54]++;
    _logger.logger.error('Failed to batch delete files', {
      userId: context.user.id,
      error: error.message
    });
    /* istanbul ignore next */
    cov_1v200j0q4j().s[55]++;
    return _server.NextResponse.json({
      success: false,
      error: 'Failed to delete files',
      message: 'Please try again later'
    }, {
      status: 500
    });
  }
})));
/**
 * Format file size for human readability
 */
function formatFileSize(bytes) {
  /* istanbul ignore next */
  cov_1v200j0q4j().f[7]++;
  cov_1v200j0q4j().s[56]++;
  if (bytes === 0) {
    /* istanbul ignore next */
    cov_1v200j0q4j().b[11][0]++;
    cov_1v200j0q4j().s[57]++;
    return '0 Bytes';
  } else
  /* istanbul ignore next */
  {
    cov_1v200j0q4j().b[11][1]++;
  }
  const k =
  /* istanbul ignore next */
  (cov_1v200j0q4j().s[58]++, 1024);
  const sizes =
  /* istanbul ignore next */
  (cov_1v200j0q4j().s[59]++, ['Bytes', 'KB', 'MB', 'GB', 'TB']);
  const i =
  /* istanbul ignore next */
  (cov_1v200j0q4j().s[60]++, Math.floor(Math.log(bytes) / Math.log(k)));
  /* istanbul ignore next */
  cov_1v200j0q4j().s[61]++;
  return parseFloat((bytes / Math.pow(k, i)).toFixed(2)) + ' ' + sizes[i];
}
