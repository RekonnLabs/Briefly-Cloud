            cov_ud05i4cwd().b[3][0]++;
            cov_ud05i4cwd().s[25]++;
            await supabase.from('users').update({
              subscription_tier: tier,
              subscription_status: 'active'
            }).eq('id', userId);
          } else
          /* istanbul ignore next */
          {
            cov_ud05i4cwd().b[3][1]++;
          }
          cov_ud05i4cwd().s[26]++;
          break;
        }
      case 'customer.subscription.deleted':
        /* istanbul ignore next */
        cov_ud05i4cwd().b[2][1]++;
        {
          const subscription =
          /* istanbul ignore next */
          (cov_ud05i4cwd().s[27]++, event.data.object);
          const customerId =
          /* istanbul ignore next */
          (cov_ud05i4cwd().s[28]++, subscription.customer);
          const {
            data: match
          } =
          /* istanbul ignore next */
          (cov_ud05i4cwd().s[29]++, await supabase.from('users').select('id').eq('stripe_customer_id', customerId).single());
          /* istanbul ignore next */
          cov_ud05i4cwd().s[30]++;
          if (match?.id) {
            /* istanbul ignore next */
            cov_ud05i4cwd().b[5][0]++;
            cov_ud05i4cwd().s[31]++;
            await supabase.from('users').update({
              subscription_tier: 'free',
              subscription_status: 'canceled'
            }).eq('id', match.id);
          } else
          /* istanbul ignore next */
          {
            cov_ud05i4cwd().b[5][1]++;
          }
          cov_ud05i4cwd().s[32]++;
          break;
        }
      default:
        /* istanbul ignore next */
        cov_ud05i4cwd().b[2][2]++;
        cov_ud05i4cwd().s[33]++;
        break;
    }
    /* istanbul ignore next */
    cov_ud05i4cwd().s[34]++;
    return _server.NextResponse.json({
      received: true
    });
  } catch (e) {
    /* istanbul ignore next */
    cov_ud05i4cwd().s[35]++;
    return _server.NextResponse.json({
      error: 'Webhook error'
    }, {
      status: 400
    });
  }
}
