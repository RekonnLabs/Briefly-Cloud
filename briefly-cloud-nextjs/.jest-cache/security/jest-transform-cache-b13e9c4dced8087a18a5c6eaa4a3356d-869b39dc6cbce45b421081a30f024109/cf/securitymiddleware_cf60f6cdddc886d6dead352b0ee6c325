  return withSecurityMonitoring({
    enableAnomalyDetection: false,
    enableThreatDetection: false,
    blockSuspiciousIPs: true,
    logAllRequests: false,
    requireSecureHeaders: false,
    enableRealTimeMonitoring: false
  }, handler);
}
