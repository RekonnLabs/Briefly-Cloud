            message: 'Request must include proper security headers'
          }, {
            status: 400
          });
        } else
        /* istanbul ignore next */
        {
          cov_d92qz2x1f().b[14][1]++;
        }
      } else
      /* istanbul ignore next */
      {
        cov_d92qz2x1f().b[13][1]++;
      }
      // Execute the handler
      const response =
      /* istanbul ignore next */
      (cov_d92qz2x1f().s[47]++, await handler(request, securityContext));
      // Log successful request if enabled
      /* istanbul ignore next */
      cov_d92qz2x1f().s[48]++;
      if (config.logAllRequests) {
        /* istanbul ignore next */
        cov_d92qz2x1f().b[15][0]++;
        cov_d92qz2x1f().s[49]++;
        await auditLogger.logAction({
          userId: context.user?.id,
          action: 'API_CALL',
          resourceType: 'api',
          metadata: {
            endpoint: request.url,
            method: request.method,
            statusCode: response.status,
            processingTime: Date.now() - startTime,
            threatLevel: securityContext.threatLevel,
            anomalyDetected:
            /* istanbul ignore next */
            (cov_d92qz2x1f().b[16][0]++, securityContext.securityAnalysis?.isAnomalous) ||
            /* istanbul ignore next */
            (cov_d92qz2x1f().b[16][1]++, false)
          },
          severity: 'info',
          ipAddress,
          userAgent
        });
      } else
      /* istanbul ignore next */
      {
        cov_d92qz2x1f().b[15][1]++;
      }
      // Add security headers to response
      cov_d92qz2x1f().s[50]++;
      addSecurityHeaders(response);
      /* istanbul ignore next */
      cov_d92qz2x1f().s[51]++;
      return response;
    } catch (error) {
      /* istanbul ignore next */
      cov_d92qz2x1f().s[52]++;
      // Log security middleware errors
      _logger.logger.error('Security middleware error', {
        url: request.url,
        method: request.method,
        userId: context.user?.id,
        error: error.message
      });
      // Don't block requests due to security middleware errors
      // Execute handler with basic security context
      const fallbackContext =
      /* istanbul ignore next */
      (cov_d92qz2x1f().s[53]++, {
        ...context,
        threatLevel: 'low',
        ipAddress: getClientIP(request),
        userAgent:
        /* istanbul ignore next */
        (cov_d92qz2x1f().b[17][0]++, request.headers.get('user-agent')) ||
        /* istanbul ignore next */
        (cov_d92qz2x1f().b[17][1]++, 'unknown'),
        requestFingerprint: generateRequestFingerprint(request)
      });
      /* istanbul ignore next */
      cov_d92qz2x1f().s[54]++;
      return handler(request, fallbackContext);
    }
  };
}
/**
 * Get client IP address from request
 */
function getClientIP(request) {
  /* istanbul ignore next */
  cov_d92qz2x1f().f[7]++;
  const forwarded =
  /* istanbul ignore next */
  (cov_d92qz2x1f().s[55]++, request.headers.get('x-forwarded-for'));
  const realIP =
  /* istanbul ignore next */
  (cov_d92qz2x1f().s[56]++, request.headers.get('x-real-ip'));
  const cfConnectingIP =
  /* istanbul ignore next */
  (cov_d92qz2x1f().s[57]++, request.headers.get('cf-connecting-ip'));
  /* istanbul ignore next */
  cov_d92qz2x1f().s[58]++;
  if (forwarded) {
    /* istanbul ignore next */
    cov_d92qz2x1f().b[18][0]++;
    cov_d92qz2x1f().s[59]++;
    return forwarded.split(',')[0].trim();
  } else
  /* istanbul ignore next */
  {
    cov_d92qz2x1f().b[18][1]++;
  }
  cov_d92qz2x1f().s[60]++;
  if (realIP) {
    /* istanbul ignore next */
    cov_d92qz2x1f().b[19][0]++;
    cov_d92qz2x1f().s[61]++;
    return realIP;
  } else
  /* istanbul ignore next */
  {
    cov_d92qz2x1f().b[19][1]++;
  }
  cov_d92qz2x1f().s[62]++;
  if (cfConnectingIP) {
    /* istanbul ignore next */
    cov_d92qz2x1f().b[20][0]++;
    cov_d92qz2x1f().s[63]++;
    return cfConnectingIP;
  } else
  /* istanbul ignore next */
  {
    cov_d92qz2x1f().b[20][1]++;
  }
  cov_d92qz2x1f().s[64]++;
  return 'unknown';
}
/**
 * Generate a unique fingerprint for the request
 */
function generateRequestFingerprint(request) {
  /* istanbul ignore next */
  cov_d92qz2x1f().f[8]++;
  const components =
  /* istanbul ignore next */
  (cov_d92qz2x1f().s[65]++, [
  /* istanbul ignore next */
  (cov_d92qz2x1f().b[21][0]++, request.headers.get('user-agent')) ||
  /* istanbul ignore next */
  (cov_d92qz2x1f().b[21][1]++, ''),
  /* istanbul ignore next */
  (cov_d92qz2x1f().b[22][0]++, request.headers.get('accept-language')) ||
  /* istanbul ignore next */
  (cov_d92qz2x1f().b[22][1]++, ''),
  /* istanbul ignore next */
  (cov_d92qz2x1f().b[23][0]++, request.headers.get('accept-encoding')) ||
  /* istanbul ignore next */
  (cov_d92qz2x1f().b[23][1]++, ''), getClientIP(request)]);
  /* istanbul ignore next */
  cov_d92qz2x1f().s[66]++;
  return Buffer.from(components.join('|')).toString('base64').substring(0, 16);
}
/**
 * Extract action from request for anomaly detection
 */
function extractActionFromRequest(request) {
  /* istanbul ignore next */
  cov_d92qz2x1f().f[9]++;
  const url =
  /* istanbul ignore next */
  (cov_d92qz2x1f().s[67]++, new URL(request.url));
  const path =
  /* istanbul ignore next */
  (cov_d92qz2x1f().s[68]++, url.pathname);
  // Map common endpoints to actions
  /* istanbul ignore next */
  cov_d92qz2x1f().s[69]++;
  if (path.includes('/chat')) {
    /* istanbul ignore next */
    cov_d92qz2x1f().b[24][0]++;
    cov_d92qz2x1f().s[70]++;
    return 'CHAT_MESSAGE';
  } else
  /* istanbul ignore next */
  {
    cov_d92qz2x1f().b[24][1]++;
  }
  cov_d92qz2x1f().s[71]++;
  if (path.includes('/upload')) {
    /* istanbul ignore next */
    cov_d92qz2x1f().b[25][0]++;
    cov_d92qz2x1f().s[72]++;
    return 'DOCUMENT_UPLOAD';
  } else
  /* istanbul ignore next */
  {
    cov_d92qz2x1f().b[25][1]++;
  }
  cov_d92qz2x1f().s[73]++;
  if (path.includes('/search')) {
    /* istanbul ignore next */
    cov_d92qz2x1f().b[26][0]++;
    cov_d92qz2x1f().s[74]++;
    return 'SEARCH_PERFORMED';
  } else
  /* istanbul ignore next */
  {
    cov_d92qz2x1f().b[26][1]++;
  }
  cov_d92qz2x1f().s[75]++;
  if (
  /* istanbul ignore next */
  (cov_d92qz2x1f().b[28][0]++, path.includes('/download')) ||
  /* istanbul ignore next */
  (cov_d92qz2x1f().b[28][1]++, path.includes('/export'))) {
    /* istanbul ignore next */
    cov_d92qz2x1f().b[27][0]++;
    cov_d92qz2x1f().s[76]++;
    return 'DATA_EXPORT';
  } else
  /* istanbul ignore next */
  {
    cov_d92qz2x1f().b[27][1]++;
  }
  cov_d92qz2x1f().s[77]++;
  if (path.includes('/admin')) {
    /* istanbul ignore next */
    cov_d92qz2x1f().b[29][0]++;
    cov_d92qz2x1f().s[78]++;
    return 'ADMIN_ACCESS';
  } else
  /* istanbul ignore next */
  {
    cov_d92qz2x1f().b[29][1]++;
  }
  cov_d92qz2x1f().s[79]++;
  return 'API_CALL';
}
/**
 * Map risk score to threat level
 */
function mapRiskScoreToThreatLevel(riskScore) {
  /* istanbul ignore next */
  cov_d92qz2x1f().f[10]++;
  cov_d92qz2x1f().s[80]++;
  if (riskScore >= 80) {
    /* istanbul ignore next */
    cov_d92qz2x1f().b[30][0]++;
    cov_d92qz2x1f().s[81]++;
    return 'critical';
  } else
  /* istanbul ignore next */
  {
    cov_d92qz2x1f().b[30][1]++;
  }
  cov_d92qz2x1f().s[82]++;
  if (riskScore >= 60) {
    /* istanbul ignore next */
    cov_d92qz2x1f().b[31][0]++;
    cov_d92qz2x1f().s[83]++;
    return 'high';
  } else
  /* istanbul ignore next */
  {
    cov_d92qz2x1f().b[31][1]++;
  }
  cov_d92qz2x1f().s[84]++;
  if (riskScore >= 30) {
    /* istanbul ignore next */
    cov_d92qz2x1f().b[32][0]++;
    cov_d92qz2x1f().s[85]++;
    return 'medium';
  } else
  /* istanbul ignore next */
  {
    cov_d92qz2x1f().b[32][1]++;
  }
  cov_d92qz2x1f().s[86]++;
  return 'low';
}
/**
 * Validate security headers
 */
function validateSecurityHeaders(request) {
  /* istanbul ignore next */
  cov_d92qz2x1f().f[11]++;
  const requiredHeaders =
  /* istanbul ignore next */
  (cov_d92qz2x1f().s[87]++, ['x-requested-with', 'origin', 'referer']);
  const missingHeaders =
  /* istanbul ignore next */
  (cov_d92qz2x1f().s[88]++, requiredHeaders.filter(header => {
    /* istanbul ignore next */
    cov_d92qz2x1f().f[12]++;
    cov_d92qz2x1f().s[89]++;
    return !request.headers.get(header);
  }));
  /* istanbul ignore next */
  cov_d92qz2x1f().s[90]++;
  return {
    valid: missingHeaders.length === 0,
    missingHeaders
  };
}
/**
 * Add security headers to response
 */
function addSecurityHeaders(response) {
  /* istanbul ignore next */
  cov_d92qz2x1f().f[13]++;
  cov_d92qz2x1f().s[91]++;
  // Add security headers
  response.headers.set('X-Content-Type-Options', 'nosniff');
  /* istanbul ignore next */
  cov_d92qz2x1f().s[92]++;
  response.headers.set('X-Frame-Options', 'DENY');
  /* istanbul ignore next */
  cov_d92qz2x1f().s[93]++;
  response.headers.set('X-XSS-Protection', '1; mode=block');
  /* istanbul ignore next */
  cov_d92qz2x1f().s[94]++;
  response.headers.set('Referrer-Policy', 'strict-origin-when-cross-origin');
  /* istanbul ignore next */
  cov_d92qz2x1f().s[95]++;
  response.headers.set('Permissions-Policy', 'camera=(), microphone=(), geolocation=()');
  // Add custom security headers
  /* istanbul ignore next */
  cov_d92qz2x1f().s[96]++;
  response.headers.set('X-Security-Scan', 'passed');
  /* istanbul ignore next */
  cov_d92qz2x1f().s[97]++;
  response.headers.set('X-Request-ID', `req_${Date.now()}_${Math.random().toString(36).substr(2, 9)}`);
}
function withHighSecurity(handler) {
  /* istanbul ignore next */
  cov_d92qz2x1f().f[14]++;
  cov_d92qz2x1f().s[98]++;
  return withSecurityMonitoring({
    enableAnomalyDetection: true,
    enableThreatDetection: true,
    blockSuspiciousIPs: true,
    logAllRequests: true,
    requireSecureHeaders: true,
    enableRealTimeMonitoring: true
  }, handler);
}
