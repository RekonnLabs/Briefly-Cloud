      const retryAfter =
      /* istanbul ignore next */
      (cov_1h3sua2ut().s[57]++, getRetryAfter(windowType, windowStart));
      /* istanbul ignore next */
      cov_1h3sua2ut().s[58]++;
      return {
        allowed: false,
        retryAfter
      };
    } else
    /* istanbul ignore next */
    {
      cov_1h3sua2ut().b[9][1]++;
    }
    // Increment counter
    cov_1h3sua2ut().s[59]++;
    await _supabase.supabaseAdmin.from('app.rate_limits').upsert({
      user_id: userId,
      limit_type: windowType,
      action,
      window_start: windowStart.toISOString(),
      count: currentCount + 1
    });
    /* istanbul ignore next */
    cov_1h3sua2ut().s[60]++;
    return {
      allowed: true
    };
  } catch (error) {
    /* istanbul ignore next */
    cov_1h3sua2ut().s[61]++;
    console.error('Rate limit check failed:', error);
    // Allow request if rate limiting fails
    /* istanbul ignore next */
    cov_1h3sua2ut().s[62]++;
    return {
      allowed: true
    };
  }
}
function withRateLimit(action, windowType =
/* istanbul ignore next */
(cov_1h3sua2ut().b[10][0]++, 'minute')) {
  /* istanbul ignore next */
  cov_1h3sua2ut().f[20]++;
  cov_1h3sua2ut().s[63]++;
  return function (handler) {
    /* istanbul ignore next */
    cov_1h3sua2ut().f[21]++;
    cov_1h3sua2ut().s[64]++;
    return withAuth(async (request, context) => {
      /* istanbul ignore next */
      cov_1h3sua2ut().f[22]++;
      const rateLimitResult =
      /* istanbul ignore next */
      (cov_1h3sua2ut().s[65]++, await checkRateLimit(context.user.id, action, windowType));
      /* istanbul ignore next */
      cov_1h3sua2ut().s[66]++;
      if (!rateLimitResult.allowed) {
        /* istanbul ignore next */
        cov_1h3sua2ut().b[11][0]++;
        cov_1h3sua2ut().s[67]++;
        return _server.NextResponse.json({
          success: false,
          error: 'RATE_LIMIT_EXCEEDED',
          message: 'Too many requests. Please try again later.',
          retryAfter: rateLimitResult.retryAfter
        }, {
          status: 429,
          headers: {
            'Retry-After':
            /* istanbul ignore next */
            (cov_1h3sua2ut().b[12][0]++, rateLimitResult.retryAfter?.toString()) ||
            /* istanbul ignore next */
            (cov_1h3sua2ut().b[12][1]++, '60'),
            'X-RateLimit-Limit': getRateLimitForAction(action, windowType).toString(),
            'X-RateLimit-Remaining': '0'
          }
        });
      } else
      /* istanbul ignore next */
      {
        cov_1h3sua2ut().b[11][1]++;
      }
      cov_1h3sua2ut().s[68]++;
      return await handler(request, context);
    });
  };
}
// Helper functions
function getWindowStart(windowType) {
  /* istanbul ignore next */
  cov_1h3sua2ut().f[23]++;
  const now =
  /* istanbul ignore next */
  (cov_1h3sua2ut().s[69]++, new Date());
  /* istanbul ignore next */
  cov_1h3sua2ut().s[70]++;
  switch (windowType) {
    case 'minute':
      /* istanbul ignore next */
      cov_1h3sua2ut().b[13][0]++;
      cov_1h3sua2ut().s[71]++;
      return new Date(now.getFullYear(), now.getMonth(), now.getDate(), now.getHours(), now.getMinutes());
    case 'hour':
      /* istanbul ignore next */
      cov_1h3sua2ut().b[13][1]++;
      cov_1h3sua2ut().s[72]++;
      return new Date(now.getFullYear(), now.getMonth(), now.getDate(), now.getHours());
    case 'day':
      /* istanbul ignore next */
      cov_1h3sua2ut().b[13][2]++;
      cov_1h3sua2ut().s[73]++;
      return new Date(now.getFullYear(), now.getMonth(), now.getDate());
    default:
      /* istanbul ignore next */
      cov_1h3sua2ut().b[13][3]++;
      cov_1h3sua2ut().s[74]++;
      return now;
  }
}
function getRateLimitForAction(action, windowType) {
  /* istanbul ignore next */
  cov_1h3sua2ut().f[24]++;
  const limits =
  /* istanbul ignore next */
  (cov_1h3sua2ut().s[75]++, {
    minute: {
      chat: 10,
      upload: 5,
      api: 60
    },
    hour: {
      chat: 100,
      upload: 50,
      api: 1000
    },
    day: {
      chat: 1000,
      upload: 100,
      api: 10000
    }
  });
  /* istanbul ignore next */
  cov_1h3sua2ut().s[76]++;
  return /* istanbul ignore next */(cov_1h3sua2ut().b[14][0]++, limits[windowType]?.[action]) ||
  /* istanbul ignore next */
  (cov_1h3sua2ut().b[14][1]++, 10);
}
function getRetryAfter(windowType, windowStart) {
  /* istanbul ignore next */
  cov_1h3sua2ut().f[25]++;
  const now =
  /* istanbul ignore next */
  (cov_1h3sua2ut().s[77]++, new Date());
  /* istanbul ignore next */
  cov_1h3sua2ut().s[78]++;
  switch (windowType) {
    case 'minute':
      /* istanbul ignore next */
      cov_1h3sua2ut().b[15][0]++;
      cov_1h3sua2ut().s[79]++;
      return 60 - now.getSeconds();
    case 'hour':
      /* istanbul ignore next */
      cov_1h3sua2ut().b[15][1]++;
      cov_1h3sua2ut().s[80]++;
      return (60 - now.getMinutes()) * 60 - now.getSeconds();
    case 'day':
      /* istanbul ignore next */
      cov_1h3sua2ut().b[15][2]++;
      cov_1h3sua2ut().s[81]++;
      return ((24 - now.getHours()) * 60 - now.getMinutes()) * 60 - now.getSeconds();
    default:
      /* istanbul ignore next */
      cov_1h3sua2ut().b[15][3]++;
      cov_1h3sua2ut().s[82]++;
      return 60;
  }
}
