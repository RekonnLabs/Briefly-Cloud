{
  "builds": [
    {
      "src": "server/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "server"
  },
  "functions": {
    "server/main.py": {
      "maxDuration": 30
    }
  },
  "installCommand": "cd server && pip install -r requirements-vercel.txt"
}